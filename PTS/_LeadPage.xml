				</WTROW>
				<WTROW>
					<WTSTATIC col="1" label="MemberTags" align="left" class="prompt"/>
				</WTROW>
				<WTROW margin-top="6" margin-bottom="6">
					<WTBUTTON col="1" merge="2" action="1" value="CONST(Update)" align="center"/>
					<WTBUTTON col="1" merge="2" action="3" value="CONST(Return)" align="center"/>
				</WTROW>
				<WTROW margin-bottom="6">
					<WTMEMO col="1" merge="2" align="left" value="ATTR(HTMLFile.Data)" cols="90" rows="40" htmleditor="editor" embedhtml="true"/>
				</WTROW>
				
				<WTROW margin-bottom="6">
