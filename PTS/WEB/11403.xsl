                                       </xsl:element>
                                       <xsl:element name="TD">
                                          <xsl:attribute name="align">left</xsl:attribute>
                                          <xsl:attribute name="valign">center</xsl:attribute>
                                          <xsl:value-of select="@subject"/>
                                             <xsl:text disable-output-escaping="yes">&amp;#160;</xsl:text>
