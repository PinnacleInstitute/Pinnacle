Function LoadProfile(list)
   On Error Resume Next

   Set oProfile = server.CreateObject("ptsProfileUser.CProfile")
   If oProfile Is Nothing Then
      DoError Err.Number, Err.Source, "Unable to Create Object - ptsProfileUser.CProfile"
   Else
      With oProfile
         .SysCurrentLanguage = reqSysLanguage
         .Load CLng(reqProfileID), CLng(reqSysUserID)
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
         If (reqProfileID = 0) Then
            .ProfileDate = Now
         End If
         If (reqMemberID = 0) Then
            reqMemberID = .MemberID
         End If
         If (.ProfileType <> 0) Then
            reqProfileType = .ProfileType
         End If
         tmpListA = .VQResponse
         tmpListB = .SQResponse
         xmlProfile = .XML(2)
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
      End With
   End If
   Set oProfile = Nothing

   Set oMember = server.CreateObject("ptsMemberUser.CMember")
   If oMember Is Nothing Then
      DoError Err.Number, Err.Source, "Unable to Create Object - ptsMemberUser.CMember"
   Else
      With oMember
         .SysCurrentLanguage = reqSysLanguage
         .Load CLng(reqMemberID), CLng(reqSysUserID)
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
         If (reqProfileType = 0) Then
            If (InStr(.Options2,"P1") <> 0) Then
               reqProfileType = 1
            End If
            If (InStr(.Options2,"P2") <> 0) Then
               reqProfileType = 2
            End If
            If (InStr(.Options2,"P3") <> 0) Then
               reqProfileType = 3
            End If
            If (InStr(.Options2,"P4") <> 0) Then
               reqProfileType = 4
            End If
            If (reqProfileType = 0) Then
               reqProfileType = 1
            End If
         End If
         xmlMember = .XML(2)
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
      End With
   End If
   Set oMember = Nothing

   Set oHTMLFile = server.CreateObject("wtHTMLFile.CHTMLFile")
   If oHTMLFile Is Nothing Then
      DoError Err.Number, Err.Source, "Unable to Create Object - wtHTMLFile.CHTMLFile"
   Else
      With oHTMLFile
         .Filename = "Profile" + CStr(list) + ".htm"
         .Path = reqSysWebDirectory + "Sections/Company/" + CStr(reqCompanyID) + "/Profile" + CStr(reqProfileType)
         .Language = reqSysLanguage
         .Project = SysProject
         .Load 
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
         xmlHTMLFile = .XML()
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
         
                  xmlHTMLFile = Replace( xmlHTMLFile, "{listA}", tmpListA )
                  xmlHTMLFile = Replace( xmlHTMLFile, "{listB}", tmpListB )
               
      End With
   End If
   Set oHTMLFile = Nothing
End Function

Function SaveProfile(list)
   On Error Resume Next
   tmpNewProfile = 0

   Set oProfile = server.CreateObject("ptsProfileUser.CProfile")
   If oProfile Is Nothing Then
      DoError Err.Number, Err.Source, "Unable to Create Object - ptsProfileUser.CProfile"
   Else
      With oProfile
         .SysCurrentLanguage = reqSysLanguage
         reqProfileID = Request.Form.Item("ProfileID")
         .Load CLng(reqProfileID), CLng(reqSysUserID)
         If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
         If (reqProfileID = 0) Then
            .MemberID = reqMemberID
            .ProfileDate = Now
            .ProfileType = reqProfileType
            .Status = 1
            tmpNewProfile = 1
         End If
         If (list = 1) Then
            .VQResponse = Request.Form.Item("ListA")
            Error = 0
         End If
         If (list = 2) Then
            .SQResponse = Request.Form.Item("ListB")
            Error = ReportAnalysis( oProfile )
            If (Error = 1) Then
               DoError -1, "", "Oops, Too few responses in first list."
            End If
            If (Error = 2) Then
               DoError -1, "", "Oops, Too few responses in second list."
            End If
            If (Error = 0) Then
               .Status = 2
            End If
         End If
         If (Error = 0) Then
            If (reqProfileID = 0) Then
               reqProfileID = CLng(.Add(CLng(reqSysUserID)))
               If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            End If
            If (reqProfileID <> 0) Then
               .Save CLng(reqSysUserID)
               If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            End If
            If (list = 2) Then
               Ret = CreateProfilePDF( reqProfileID, 1 )
               
                If err.number <> 0 Then
                  response.write err.description
                  response.end
                End If
              
            End If
         End If
      End With
   End If
   Set oProfile = Nothing

   If (Error = 0) And (tmpNewProfile <> 0) Then
      Set oMember = server.CreateObject("ptsMemberUser.CMember")
      If oMember Is Nothing Then
         DoError Err.Number, Err.Source, "Unable to Create Object - ptsMemberUser.CMember"
      Else
         With oMember
            .SysCurrentLanguage = reqSysLanguage
            .Load CLng(reqMemberID), CLng(reqSysUserID)
            If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            GroupID = .GroupID
            Credit = CLNG(.Process)
            If (Credit > 0) Then
               .Process = Credit-1
               .Save CLng(reqSysUserID)
               If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            End If
         End With
      End If
      Set oMember = Nothing
   End If

   If (Error = 0) And (tmpNewProfile <> 0) And (GroupID <> 0) And (Credit = 0) Then
      Set oMember = server.CreateObject("ptsMemberUser.CMember")
      If oMember Is Nothing Then
         DoError Err.Number, Err.Source, "Unable to Create Object - ptsMemberUser.CMember"
      Else
         With oMember
            .SysCurrentLanguage = reqSysLanguage
            .Load GroupID, CLng(reqSysUserID)
            If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            Credit = CLNG(.Process)
            If (Credit > 0) Then
               .Process = Credit-1
               .Save CLng(reqSysUserID)
               If (Err.Number <> 0) Then DoError Err.Number, Err.Source, Err.Description End If
            End If
         End With
      End If
      Set oMember = Nothing
   End If
End Function

