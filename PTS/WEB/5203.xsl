                                             <xsl:if test="(@addresstype = 2)">
                                                <xsl:value-of select="/DATA/LANGUAGE/LABEL[@name='AddressType2']"/>
                                             </xsl:if>
                                             <xsl:if test="(@addresstype = 3)">
                                                <xsl:value-of select="/DATA/LANGUAGE/LABEL[@name='AddressType3']"/>
