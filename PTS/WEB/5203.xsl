                                    <xsl:value-of select="@zip"/>
                                    <xsl:text disable-output-escaping="yes">&amp;#160;</xsl:text>
                                    <xsl:value-of select="@countryname"/>
                                 </xsl:element>
                              </xsl:element>
                           </xsl:element>
