                  <xsl:text disable-output-escaping="yes"><![CDATA[ function AddTimeStamp(){ 
               var d = new Date();
               var year = d.getFullYear()
               if(year<1000) year+=1900
               var curHour = d.getHours();
               var curMin = d.getMinutes();
               var curAMPM = "am ";
               var curTime = "";
               if (curHour >= 12){
               curHour -= 12;
               curAMPM = "pm ";
               }
               if (curHour == 0) curHour = 12;
               curTime = curHour + ":" + ((curMin < 10) ? "0" : "") + curMin + curAMPM
               document.getElementById('Notes').value = document.getElementById('Notes').value + "\n" + (d.getMonth()+1) + "/" + d.getDate() + "/" + (year+"").substring(2,4) + " " + curTime
             }]]></xsl:text>
