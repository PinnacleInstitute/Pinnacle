		<!--============================================================================================-->
		<WTWEBPAGE name="6104" caption="Personalization" navbar="false" header="false" footer="false" 
		aspstyle="GenerateWebASP.xsl" xslstyle="GenerateWebXSLList.xsl" >
		<!--============================================================================================-->
			<WTPARAM name="CompanyID" datatype="number"/>
			<WTPARAM name="MemberID" datatype="number"/>
			<WTPARAM name="LeadCampaignID" datatype="number"/>
			<WTPARAM name="Welcome" datatype="number"/>
			<WTPARAM name="Story" datatype="number"/>
			<WTPARAM name="Vision" datatype="number"/>
			<WTPARAM name="Testimony" datatype="number"/>
			<WTPARAM name="Invite" datatype="number"/>
			<WTPARAM name="Language" datatype="text"/>

			<WTCODEGROUP>
				<WTSETATTRIBUTE name="PARAM(Language)" value="SYS(Language)"/>
			</WTCODEGROUP>

			<WTSUB name="LoadList">
				<WTGETCACHE name="CONST(tmpGroupID)" value="GROUPID"/>
				<WTOBJECT name="LeadCampaigns" project="ptsLeadCampaignUser" class="CLeadCampaigns">
					<WTCONDITION expr="PARAM(LeadCampaignID)" oper="not-equal" value="CONST(0)"/>
					<WTMETHOD name="EnumPresent">
						<WTPARAM name="xmlLeadCampaigns" direction="return" datatype="text"/>
						<WTPARAM name="CompanyID" direction="input" value="PARAM(CompanyID)"/>
						<WTPARAM name="Grp" direction="input" value="CONST(tmpGroupID)" />
						<WTPARAM name="MemberID" direction="input" value="PARAM(MemberID)" />
						<WTPARAM name="ItemID" direction="input" value="CONST()"/>
						<WTPARAM name="Element" direction="input" value="CONST()"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
<WTCUSTOM>
xmlLeadCampaigns = Replace( xmlLeadCampaigns, "name=" + CHR(34) + " " + CHR(34), "name=" + CHR(34) + "Personal Info for All Presentations" + CHR(34)) 
</WTCUSTOM>					
				</WTOBJECT>
			</WTSUB>

			<WTSUB name="LoadPersonal">
				<WTOBJECT name="LeadCampaign" project="ptsLeadCampaignUser" class="CLeadCampaign">
					<WTCONDITION expr="PARAM(LeadCampaignID)" oper="not-equal" value="CONST(0)"/>
					<WTMETHOD name="Load">
						<WTPARAM name="LeadCampaignID" direction="input" value="PARAM(LeadCampaignID)" />
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)" />
					</WTMETHOD>
					<WTMETHOD name="XML">
						<WTPARAM name="xmlLeadCampaign" direction="return" datatype="text" />
						<WTPARAM name="Option" direction="input" value="CONST(2)" />
					</WTMETHOD>
				</WTOBJECT>
				<WTOBJECT name="Signatures" project="ptsSignatureUser" class="CSignatures">
					<WTMETHOD name="ListPersonal">
						<WTPARAM name="MemberID" direction="input" value="PARAM(MemberID)"/>
						<WTPARAM name="UserID" direction="input" value="PARAM(LeadCampaignID)"/>
						<WTPARAM name="Language" direction="input" value="PARAM(Language)"/>
					</WTMETHOD>
<WTCUSTOM>
		For Each oItem in oSignatures
			Select Case oItem.UseType
				Case 7: reqWelcome = 1
				Case 8: reqStory = 1
				Case 9: reqVision = 1
				Case 10: reqTestimony = 1
				Case 11: reqInvite = 1
			End Select	
		Next
</WTCUSTOM>
					<WTMETHOD name="XML">
						<WTPARAM name="xmlSignatures" direction="return" datatype="text"/>
						<WTPARAM name="Option" direction="input" value="CONST(13)"/>
					</WTMETHOD>
				</WTOBJECT>
				<WTCALLSUB name="LoadList"/>
			</WTSUB>

			<WTACTION name="New" type="New" id="0">
				<WTCALLSUB name="LoadPersonal"/>
			</WTACTION>

			<WTACTION id="4" name="Delete" type="Delete">
				<WTSETATTRIBUTE name="CONST(tmpSignatureID)" value="FORM(SignatureID)"/>
				<WTOBJECT name="Signature" project="ptsSignatureUser" class="CSignature">
					<WTMETHOD name="Delete">
						<WTPARAM name="SignatureID" direction="input" value="CONST(tmpSignatureID)"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
				</WTOBJECT>
				<WTCALLSUB name="LoadPersonal"/>
			</WTACTION>

			<WTACTION id="5" name="Copy" type="Update">
				<WTSETATTRIBUTE name="CONST(tmpCopy)" value="FORM(CopyLeadCampaign)"/>
				<WTOBJECT name="Signature" project="ptsSignatureUser" class="CSignature">
					<WTMETHOD name="Copy">
						<WTPARAM name="MemberID" direction="input" value="PARAM(MemberID)"/>
						<WTPARAM name="UserID" direction="input" value="PARAM(LeadCampaignID)"/>
						<WTPARAM name="Language" direction="input" value="PARAM(Language)"/>
						<WTPARAM name="CopyID" direction="input" value="CONST(tmpCopy)"/>
						<WTPARAM name="Result" direction="return" datatype="number"/>
					</WTMETHOD>
				</WTOBJECT>
				<WTCALLSUB name="LoadPersonal"/>
			</WTACTION>

	 		<!--************************************************************************************-->
			<WTCONTENT>
				<WTCOLUMN width="600" align="left"/>

				<WTFUNCTION name="Edit(id,usetype)">
					var memberid = document.getElementById('MemberID').value;
					var useid = document.getElementById('LeadCampaignID').value;
					var language = document.getElementById('Language').value;
					var url, win;
					url = "6105.asp?signatureid=" + id + "&amp;memberid=" + memberid + "&amp;usetype=" + usetype + "&amp;useid=" + useid + "&amp;language=" + language
		            win = window.open(url,"Personal");
					win.focus();
			    </WTFUNCTION>  
				<WTFUNCTION name="Remove(id)">
					document.getElementById('SignatureID').value = id;
					doSubmit(4,"");
			    </WTFUNCTION>  

				<WTROW>
					<WTHIDDEN value="PARAM(MemberID)"/>
					<WTHIDDEN value="PARAM(LeadCampaignID)"/>
					<WTHIDDEN value="PARAM(Language)"/>
					<WTHIDDEN value="ATTR(Signature.SignatureID)"/>
