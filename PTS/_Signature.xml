	<WTENTITY id="61" name="Signature" alias="sr">

		<!--Identity-->
		<WTATTRIBUTE id="6101" name="SignatureID" type="number" identity="true" min="1" title="true" required="true" source="entity"/>
		<WTATTRIBUTE id="6102" name="MemberID" type="number" required="true" source="entity"/>

		<!--Attributes-->
		<WTATTRIBUTE id="6103" name="UseType" type="number" source="entity">
			<WTENUM id="1" name="Default"/>
			<WTENUM id="2" name="LeadPage"/>
			<WTENUM id="3" name="Presentation"/>
			<WTENUM id="4" name="LeadEmail"/>
			<WTENUM id="5" name="ProspectEmail"/>
			<WTENUM id="6" name="CustomerEmail"/>
			<WTENUM id="7" name="Welcome"/>
			<WTENUM id="8" name="Story"/>
			<WTENUM id="9" name="Vision"/>
			<WTENUM id="10" name="Testimony"/>
			<WTENUM id="11" name="Invite"/>
		</WTATTRIBUTE>
		<WTATTRIBUTE id="6104" name="UseID" type="number" source="entity"/>
		<WTATTRIBUTE id="6105" name="IsActive" type="yesno" source="entity"/>
		<WTATTRIBUTE id="6106" name="Data" type="text" length="4000" min="1" max="4000" source="entity" embedhtml="true"/>
		<WTATTRIBUTE id="6107" name="Language" type="text" length="6" min="1" max="6" source="entity" language="false"/>				

		<!-- Non Persist Fields -->
		<WTATTRIBUTE id="6190" name="CopyID" type="number" source="entity" persist="false"/>

		<WTINDEX name="MemberID">
			<WTATTRIBUTE name="MemberID"/>
			<WTATTRIBUTE name="UseType"/>
			<WTATTRIBUTE name="UseID"/>
		</WTINDEX>

		<WTRELATIONSHIPS>
			<WTRELATIONSHIP name="Signature" entity="Signature" alias="sr"/>
		</WTRELATIONSHIPS>

		<!-- System Constants --> 
		<WTSYSCONS>
			<WTSYSCON name="SIG-TYPE-DEFAULT" value="1"/>
			<WTSYSCON name="SIG-TYPE-LEADPAGE" value="2"/>
			<WTSYSCON name="SIG-TYPE-PRESENT" value="3"/>
			<WTSYSCON name="SIG-TYPE-LEADEMAIL" value="4"/>
			<WTSYSCON name="SIG-TYPE-PROSPECTEMAIL" value="5"/>
			<WTSYSCON name="SIG-TYPE-CUSTOMEREMAIL" value="6"/>
			<WTSYSCON name="SIG-TYPE-WELCOME" value="7"/>
			<WTSYSCON name="SIG-TYPE-STORY" value="8"/>
			<WTSYSCON name="SIG-TYPE-VISION" value="9"/>
			<WTSYSCON name="SIG-TYPE-TESTIMONY" value="10"/>
