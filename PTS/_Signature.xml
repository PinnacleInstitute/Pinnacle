			<WTPARAM name="ATTR(Language)" direction="input" required="false"/>
			<WTPARAM name="ATTR(CopyID)" direction="input" required="false" passthru="true"/>
			<WTPARAM name="Result" direction="output" datatype="number"/>
		</WTPROCEDURE>
	</WTPROCEDURES>

