			<WTSUB name="LoadLeadPage">
				<WTOBJECT name="Signature" project="ptsSignatureUser" class="CSignature">
					<WTCONDITION expr="PARAM(SignatureID)" oper="not-equal" value="CONST(0)" />
					<WTMETHOD name="Load">
						<WTPARAM name="SignatureID" direction="input" value="PARAM(SignatureID)"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
					<WTMETHOD name="XML">
						<WTPARAM name="xmlSignature" direction="return" datatype="text"/>
						<WTPARAM name="Option" direction="input" value="CONST(2)"/>
					</WTMETHOD>
				</WTOBJECT>
			</WTSUB>
			
			<WTSUB name="UpdateLeadPage">
				<WTOBJECT name="Signature" project="ptsSignatureUser" class="CSignature">
					<WTCONDITION expr="PARAM(SignatureID)" oper="not-equal" value="CONST(0)" />
					<WTMETHOD name="Load">
						<WTPARAM name="SignatureID" direction="input" value="PARAM(SignatureID)"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
					<WTSETATTRIBUTE name="ATTR(Data)" value="FORM(Data)"/>
					<WTMETHOD name="Save">
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
					<WTMETHOD name="XML">
						<WTPARAM name="xmlSignature" direction="return" datatype="text"/>
						<WTPARAM name="Option" direction="input" value="CONST(2)"/>
					</WTMETHOD>
				</WTOBJECT>
				<WTOBJECT name="Signature" project="ptsSignatureUser" class="CSignature">
					<WTCONDITION expr="PARAM(SignatureID)" oper="equal" value="CONST(0)" />
					<WTMETHOD name="Load">
						<WTPARAM name="SignatureID" direction="input" value="CONST(0)"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
					<WTSETATTRIBUTE name="ATTR(MemberID)" value="PARAM(MemberID)"/>
					<WTSETATTRIBUTE name="ATTR(UseType)" value="PARAM(UseType)"/>
					<WTSETATTRIBUTE name="ATTR(UseID)" value="PARAM(UseID)"/>
					<WTSETATTRIBUTE name="ATTR(Language)" value="PARAM(Language)"/>
					<WTSETATTRIBUTE name="ATTR(IsActive)" value="CONST(1)"/>
					<WTSETATTRIBUTE name="ATTR(Data)" value="FORM(Data)"/>
					<WTMETHOD name="Add">
						<WTPARAM name="reqSignatureID" direction="return" datatype="number"/>
						<WTPARAM name="SecurityToken" direction="input" value="SYS(userid)"/>
					</WTMETHOD>
					<WTMETHOD name="XML">
						<WTPARAM name="xmlSignature" direction="return" datatype="text"/>
						<WTPARAM name="Option" direction="input" value="CONST(2)"/>
					</WTMETHOD>
				</WTOBJECT>
				<WTSETATTRIBUTE name="PARAM(Txn)" value="CONST(1)">
					<WTCONDITION expr="NoErrors"/>
				</WTSETATTRIBUTE>
			</WTSUB>
			
			<WTACTION id="0" name="New" type="New">
				<WTCALLSUB name="LoadLeadPage"/>
			</WTACTION>
			
			<WTACTION id="1" name="Update" type="Update">
				<WTCALLSUB name="UpdateLeadPage"/>
			</WTACTION>

