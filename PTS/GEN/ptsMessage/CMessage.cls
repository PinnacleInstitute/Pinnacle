Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Message") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(8521).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (8521).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(MessageID) > 0) Then sAttributes = sAttributes + XMLAttribute("MessageID", MessageID, False)
      If (Len(ForumID) > 0) Then sAttributes = sAttributes + XMLAttribute("ForumID", ForumID, False)
      If (Len(ParentID) > 0) Then sAttributes = sAttributes + XMLAttribute("ParentID", ParentID, False)
      If (Len(ThreadID) > 0) Then sAttributes = sAttributes + XMLAttribute("ThreadID", ThreadID, False)
      If (Len(BoardUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserID", BoardUserID, False)
      If (Len(ModifyID) > 0) Then sAttributes = sAttributes + XMLAttribute("ModifyID", ModifyID, False)
      If (Len(ForumName) > 0) Then sAttributes = sAttributes + XMLAttribute("ForumName", ForumName, False)
      If (Len(BoardUserName) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserName", BoardUserName, False)
      If (Len(Signature) > 0) Then sChildren = sChildren + XMLElement("Signature", "", "<!-- " + Replace(Signature, "--", "- ") + " -->", "")
      If (Len(ModifyName) > 0) Then sAttributes = sAttributes + XMLAttribute("ModifyName", ModifyName, False)
      If (Len(MessageTitle) > 0) Then sAttributes = sAttributes + XMLAttribute("MessageTitle", MessageTitle, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(IsSticky) > 0) Then sAttributes = sAttributes + XMLAttribute("IsSticky", IsSticky, False)
      If (Len(Body) > 0) Then sChildren = sChildren + XMLElement("Body", "", "<!-- " + Replace(Body, "--", "- ") + " -->", "")
      If (Len(CreateDate) > 0) And (CreateDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CreateDate", Format$(CreateDate, "m-d-yy h:mm am/pm"), False)
      If (Len(ChangeDate) > 0) And (ChangeDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ChangeDate", Format$(ChangeDate, "m-d-yy h:mm am/pm"), False)
      If (Len(ThreadOrder) > 0) Then sAttributes = sAttributes + XMLAttribute("ThreadOrder", ThreadOrder, False)
      If (Len(Replies) > 0) Then sAttributes = sAttributes + XMLAttribute("Replies", Replies, False)
      If (Len(PageSize) > 0) Then sAttributes = sAttributes + XMLAttribute("PageSize", PageSize, False)
      If (Len(Page) > 0) Then sAttributes = sAttributes + XMLAttribute("Page", Page, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

