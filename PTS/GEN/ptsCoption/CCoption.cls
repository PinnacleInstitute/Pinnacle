      If (Len(Options5) > 0) Then sAttributes = sAttributes + XMLAttribute("Options5", Options5, False)
      If (Len(Options6) > 0) Then sAttributes = sAttributes + XMLAttribute("Options6", Options6, False)
      If (Len(Options7) > 0) Then sAttributes = sAttributes + XMLAttribute("Options7", Options7, False)
      If (Len(Options8) > 0) Then sAttributes = sAttributes + XMLAttribute("Options8", Options8, False)
      If (Len(FreeName) > 0) Then sAttributes = sAttributes + XMLAttribute("FreeName", FreeName, False)
      If (Len(PaidName) > 0) Then sAttributes = sAttributes + XMLAttribute("PaidName", PaidName, False)
      If (Len(PaidName2) > 0) Then sAttributes = sAttributes + XMLAttribute("PaidName2", PaidName2, False)
      If (Len(PaidName3) > 0) Then sAttributes = sAttributes + XMLAttribute("PaidName3", PaidName3, False)
      If (Len(Identify) > 0) Then sAttributes = sAttributes + XMLAttribute("Identify", Identify, False)
      If (Len(Identification) > 0) Then sAttributes = sAttributes + XMLAttribute("Identification", Identification, False)
      If (Len(MerchantCardType) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantCardType", MerchantCardType, False)
