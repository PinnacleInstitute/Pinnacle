   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCoptionEnumBilling
         oEnums.Add cptsCoptionBillingBillNone, "BillNone"
         oEnums.Add cptsCoptionBillingBillCompany, "BillCompany"
         oEnums.Add cptsCoptionBillingBillMember, "BillMember"
         oEnums.Add cptsCoptionBillingBillManual, "BillManual"
