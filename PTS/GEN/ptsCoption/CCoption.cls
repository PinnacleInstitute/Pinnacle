         oEnums.Add cptsCoptionProjectsDefaultpNone, "pNone"
         oEnums.Add cptsCoptionProjectsDefaultpAssign, "pAssign"
         oEnums.Add cptsCoptionProjectsDefaultpSecure, "pSecure"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

