   End With

   '-----execute the command
   RunSP oCmd, moSys.ConnectString("Pinnacle", "pts"), "pts_QuestionType_Add"

   '-----get return values
   With brRec
      .QuestionTypeID = moUtil.FetchLong(oCmd.Parameters.Item("@QuestionTypeID").Value)
