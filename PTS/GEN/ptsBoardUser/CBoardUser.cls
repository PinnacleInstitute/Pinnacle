Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "BoardUser") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(BoardUserGroup) Then
         sChildren = sChildren + EnumItems(8313).XML("ptsBoardUserGroups", BoardUserGroup)
      Else
         sChildren = sChildren + EnumItems (8313).XML("ptsBoardUserGroups")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(BoardUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserID", BoardUserID, False)
      If (Len(AuthUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserID", AuthUserID, False)
      If (Len(AuthUserNameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserNameLast", AuthUserNameLast, False)
      If (Len(AuthUserNameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserNameFirst", AuthUserNameFirst, False)
      If (Len(AuthUserName) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserName", AuthUserName, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(BoardUserPassword) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserPassword", BoardUserPassword, False)
      If (Len(BoardUserName) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserName", BoardUserName, False)
      If (Len(BoardUserGroup) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardUserGroup", BoardUserGroup, False)
      If (Len(IsPublicName) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPublicName", IsPublicName, False)
      If (Len(IsPublicEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPublicEmail", IsPublicEmail, False)
      If (Len(Signature) > 0) Then sChildren = sChildren + XMLElement("Signature", "", "<!-- " + Replace(Signature, "--", "- ") + " -->", "")
      If (Len(ForumID) > 0) Then sAttributes = sAttributes + XMLAttribute("ForumID", ForumID, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

