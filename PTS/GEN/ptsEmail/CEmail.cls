Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Email") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(8809).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (8809).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(EmailID) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailID", EmailID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(NewsLetterID) > 0) Then sAttributes = sAttributes + XMLAttribute("NewsLetterID", NewsLetterID, False)
      If (Len(EmailListID) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailListID", EmailListID, False)
      If (Len(EmailName) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailName", EmailName, False)
      If (Len(FromEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("FromEmail", FromEmail, False)
      If (Len(Subject) > 0) Then sAttributes = sAttributes + XMLAttribute("Subject", Subject, False)
      If (Len(FileName) > 0) Then sAttributes = sAttributes + XMLAttribute("FileName", FileName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(SendDate) > 0) And (SendDate <> "0") Then sAttributes = sAttributes + XMLAttribute("SendDate", Format$(SendDate, "m/d/yyyy"), False)
      If (Len(Repeat) > 0) Then sAttributes = sAttributes + XMLAttribute("Repeat", Repeat, False)
      If (Len(EndDate) > 0) And (EndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EndDate", Format$(EndDate, "m/d/yyyy"), False)
      If (Len(Mailings) > 0) Then sAttributes = sAttributes + XMLAttribute("Mailings", Mailings, False)
      If (Len(Emails) > 0) Then sAttributes = sAttributes + XMLAttribute("Emails", Emails, False)
      If (Len(TestEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("TestEmail", TestEmail, False)
      If (Len(TestFirstName) > 0) Then sAttributes = sAttributes + XMLAttribute("TestFirstName", TestFirstName, False)
      If (Len(TestLastName) > 0) Then sAttributes = sAttributes + XMLAttribute("TestLastName", TestLastName, False)
      If (Len(TestData1) > 0) Then sAttributes = sAttributes + XMLAttribute("TestData1", TestData1, False)
      If (Len(TestData2) > 0) Then sAttributes = sAttributes + XMLAttribute("TestData2", TestData2, False)
      If (Len(TestData3) > 0) Then sAttributes = sAttributes + XMLAttribute("TestData3", TestData3, False)
      If (Len(TestData4) > 0) Then sAttributes = sAttributes + XMLAttribute("TestData4", TestData4, False)
      If (Len(TestData5) > 0) Then sAttributes = sAttributes + XMLAttribute("TestData5", TestData5, False)
      If (Len(EmailType) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailType", EmailType, False)
      If (Len(IsSalesStep) > 0) Then sAttributes = sAttributes + XMLAttribute("IsSalesStep", IsSalesStep, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

