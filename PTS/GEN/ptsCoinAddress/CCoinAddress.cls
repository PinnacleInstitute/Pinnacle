Public Property Get EnumItems( _ 
   ByVal bvType As ptsCoinAddressEnumConstants) As wtSystem.CEnumItems
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns an enumerated list of items.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCoinAddressEnumCoin
         oEnums.Add cptsCoinAddressCoinCoin1, "Coin1"
         oEnums.Add cptsCoinAddressCoinCoin2, "Coin2"
         oEnums.Add cptsCoinAddressCoinCoin3, "Coin3"
         oEnums.Add cptsCoinAddressCoinCoin4, "Coin4"
         oEnums.Add cptsCoinAddressCoinCoin5, "Coin5"
         oEnums.Add cptsCoinAddressCoinCoin6, "Coin6"
         oEnums.Add cptsCoinAddressCoinCoin7, "Coin7"
         oEnums.Add cptsCoinAddressCoinCoin8, "Coin8"
         oEnums.Add cptsCoinAddressCoinCoin9, "Coin9"
         oEnums.Add cptsCoinAddressCoinCoin10, "Coin10"
      Case cptsCoinAddressEnumStatus
         oEnums.Add cptsCoinAddressStatusActive, "Active"
         oEnums.Add cptsCoinAddressStatusInactive, "Inactive"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

