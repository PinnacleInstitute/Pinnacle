   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsQuestionEnumStatus
         oEnums.Add cptsQuestionStatusPending, "Pending"
         oEnums.Add cptsQuestionStatusActive, "Active"
