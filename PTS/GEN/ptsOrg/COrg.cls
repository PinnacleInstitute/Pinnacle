         oEnums.Add cptsOrgStatusActive, "Active"
         oEnums.Add cptsOrgStatusDemo, "Demo"
         oEnums.Add cptsOrgStatusInactive, "Inactive"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

