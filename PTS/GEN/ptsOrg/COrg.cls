Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Org") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(UserGroup) Then
         sChildren = sChildren + EnumItems(2810).XML("ptsUserGroups", UserGroup)
      Else
         sChildren = sChildren + EnumItems (2810).XML("ptsUserGroups")
      End If

      If IsNumeric(UserStatus) Then
         sChildren = sChildren + EnumItems(2811).XML("ptsUserStatuss", UserStatus)
      Else
         sChildren = sChildren + EnumItems (2811).XML("ptsUserStatuss")
      End If

      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(2817).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (2817).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(OrgID) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgID", OrgID, False)
      If (Len(AuthUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserID", AuthUserID, False)
      If (Len(ParentID) > 0) Then sAttributes = sAttributes + XMLAttribute("ParentID", ParentID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(ForumID) > 0) Then sAttributes = sAttributes + XMLAttribute("ForumID", ForumID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(PrivateID) > 0) Then sAttributes = sAttributes + XMLAttribute("PrivateID", PrivateID, False)
      If (Len(UserGroup) > 0) Then sAttributes = sAttributes + XMLAttribute("UserGroup", UserGroup, False)
      If (Len(UserStatus) > 0) Then sAttributes = sAttributes + XMLAttribute("UserStatus", UserStatus, False)
      If (Len(Logon) > 0) Then sAttributes = sAttributes + XMLAttribute("Logon", Logon, False)
      If (Len(OrgName) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgName", OrgName, False)
      If (Len(Description) > 0) Then sChildren = sChildren + XMLElement("Description", "", "<!-- " + Replace(Description, "--", "- ") + " -->", "")
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(ContactName) > 0) Then sAttributes = sAttributes + XMLAttribute("ContactName", ContactName, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(IsCatalog) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCatalog", IsCatalog, False)
      If (Len(Level) > 0) Then sAttributes = sAttributes + XMLAttribute("Level", Level, False)
      If (Len(Hierarchy) > 0) Then sAttributes = sAttributes + XMLAttribute("Hierarchy", Hierarchy, False)
      If (Len(CourseCount) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseCount", CourseCount, False)
      If (Len(MemberCount) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberCount", MemberCount, False)
      If (Len(IsPublic) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPublic", IsPublic, False)
      If (Len(IsChat) > 0) Then sAttributes = sAttributes + XMLAttribute("IsChat", IsChat, False)
      If (Len(IsForum) > 0) Then sAttributes = sAttributes + XMLAttribute("IsForum", IsForum, False)
      If (Len(IsSuggestion) > 0) Then sAttributes = sAttributes + XMLAttribute("IsSuggestion", IsSuggestion, False)
      If (Len(IsFavorite) > 0) Then sAttributes = sAttributes + XMLAttribute("IsFavorite", IsFavorite, False)
      If (Len(IsProgram) > 0) Then sAttributes = sAttributes + XMLAttribute("IsProgram", IsProgram, False)
      If (Len(NoCertificate) > 0) Then sAttributes = sAttributes + XMLAttribute("NoCertificate", NoCertificate, False)
      If (Len(IsCustomCertificate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCustomCertificate", IsCustomCertificate, False)
      If (Len(Secure) > 0) Then sAttributes = sAttributes + XMLAttribute("Secure", Secure, False)
      If (Len(Credits) > 0) Then sAttributes = sAttributes + XMLAttribute("Credits", Credits, False)
      If (Len(NewLogon) > 0) Then sAttributes = sAttributes + XMLAttribute("NewLogon", NewLogon, False)
      If (Len(NewPassword) > 0) Then sAttributes = sAttributes + XMLAttribute("NewPassword", NewPassword, False)
      If (Len(CourseDate) > 0) And (CourseDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CourseDate", Format$(CourseDate, "m/d/yyyy"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

