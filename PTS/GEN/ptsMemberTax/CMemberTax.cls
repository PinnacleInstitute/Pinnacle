Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "MemberTax") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(VehicleMethod) Then
         sChildren = sChildren + EnumItems(6706).XML("ptsVehicleMethods", VehicleMethod, 0)
      Else
         sChildren = sChildren + EnumItems (6706).XML("ptsVehicleMethods", , 0)
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(MemberTaxID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberTaxID", MemberTaxID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(Year) > 0) Then sAttributes = sAttributes + XMLAttribute("Year", Year, False)
      If (Len(VehicleMethod) > 0) Then sAttributes = sAttributes + XMLAttribute("VehicleMethod", VehicleMethod, False)
      If (Len(MilesStart) > 0) Then sAttributes = sAttributes + XMLAttribute("MilesStart", Format$(MilesStart, "###,##0"), False)
      If (Len(MilesEnd) > 0) Then sAttributes = sAttributes + XMLAttribute("MilesEnd", Format$(MilesEnd, "###,##0"), False)
      If (Len(TotalMiles) > 0) Then sAttributes = sAttributes + XMLAttribute("TotalMiles", Format$(TotalMiles, "###,##0"), False)
      If (Len(BusMiles) > 0) Then sAttributes = sAttributes + XMLAttribute("BusMiles", Format$(BusMiles, "###,##0"), False)
      If (Len(VehicleRate) > 0) Then sAttributes = sAttributes + XMLAttribute("VehicleRate", VehicleRate, False)
      If (Len(TotalSpace) > 0) Then sAttributes = sAttributes + XMLAttribute("TotalSpace", Format$(TotalSpace, "###,##0"), False)
      If (Len(BusSpace) > 0) Then sAttributes = sAttributes + XMLAttribute("BusSpace", Format$(BusSpace, "###,##0"), False)
      If (Len(SpaceRate) > 0) Then sAttributes = sAttributes + XMLAttribute("SpaceRate", SpaceRate, False)
      If (Len(Notes) > 0) Then sAttributes = sAttributes + XMLAttribute("Notes", Notes, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

