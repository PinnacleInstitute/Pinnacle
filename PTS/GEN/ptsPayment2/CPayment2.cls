Public Property Get EnumItems( _ 
   ByVal bvType As ptsPayment2EnumConstants) As wtSystem.CEnumItems
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns an enumerated list of items.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsPayment2EnumPayType
         oEnums.Add cptsPayment2PayTypeCash, "Cash"
         oEnums.Add cptsPayment2PayTypeCredit, "Credit"
         oEnums.Add cptsPayment2PayTypeCoin1, "Coin1"
         oEnums.Add cptsPayment2PayTypeCoin2, "Coin2"
         oEnums.Add cptsPayment2PayTypeCoin3, "Coin3"
         oEnums.Add cptsPayment2PayTypeCoin4, "Coin4"
         oEnums.Add cptsPayment2PayTypeCoin5, "Coin5"
         oEnums.Add cptsPayment2PayTypeCoin6, "Coin6"
         oEnums.Add cptsPayment2PayTypeCoin7, "Coin7"
         oEnums.Add cptsPayment2PayTypeCoin8, "Coin8"
         oEnums.Add cptsPayment2PayTypeCoin9, "Coin9"
         oEnums.Add cptsPayment2PayTypeCoin10, "Coin10"
      Case cptsPayment2EnumStatus
         oEnums.Add cptsPayment2StatusSubmitted, "Submitted"
         oEnums.Add cptsPayment2StatusPending, "Pending"
         oEnums.Add cptsPayment2StatusApproved, "Approved"
         oEnums.Add cptsPayment2StatusPartial, "Partial"
         oEnums.Add cptsPayment2StatusDeclined, "Declined"
         oEnums.Add cptsPayment2StatusReturned, "Returned"
         oEnums.Add cptsPayment2StatusCancelled, "Cancelled"
      Case cptsPayment2EnumCommStatus
         oEnums.Add cptsPayment2CommStatusNo, "No"
         oEnums.Add cptsPayment2CommStatusYes, "Yes"
         oEnums.Add cptsPayment2CommStatusNone, "None"
         oEnums.Add cptsPayment2CommStatusReclaimed, "Reclaimed"
      Case cptsPayment2EnumCoinStatus
         oEnums.Add cptsPayment2CoinStatusCoinNotSent, "CoinNotSent"
         oEnums.Add cptsPayment2CoinStatusCoinSent, "CoinSent"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

