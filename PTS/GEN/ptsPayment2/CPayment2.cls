Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Payment2") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(PayType) Then
         sChildren = sChildren + EnumItems(16516).XML("ptsPayTypes", PayType)
      Else
         sChildren = sChildren + EnumItems (16516).XML("ptsPayTypes")
      End If

      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(16517).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (16517).XML("ptsStatuss")
      End If

      If IsNumeric(CommStatus) Then
         sChildren = sChildren + EnumItems(16530).XML("ptsCommStatuss", CommStatus)
      Else
         sChildren = sChildren + EnumItems (16530).XML("ptsCommStatuss")
      End If

      If IsNumeric(CoinStatus) Then
         sChildren = sChildren + EnumItems(16532).XML("ptsCoinStatuss", CoinStatus)
      Else
         sChildren = sChildren + EnumItems (16532).XML("ptsCoinStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(Payment2ID) > 0) Then sAttributes = sAttributes + XMLAttribute("Payment2ID", Payment2ID, False)
      If (Len(MerchantID) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantID", MerchantID, False)
      If (Len(ConsumerID) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerID", ConsumerID, False)
      If (Len(StaffID) > 0) Then sAttributes = sAttributes + XMLAttribute("StaffID", StaffID, False)
      If (Len(AwardID) > 0) Then sAttributes = sAttributes + XMLAttribute("AwardID", AwardID, False)
      If (Len(SalesOrderID) > 0) Then sAttributes = sAttributes + XMLAttribute("SalesOrderID", SalesOrderID, False)
      If (Len(StatementID) > 0) Then sAttributes = sAttributes + XMLAttribute("StatementID", StatementID, False)
      If (Len(MerchantName) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantName", MerchantName, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(ConsumerName) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerName", ConsumerName, False)
      If (Len(StaffName) > 0) Then sAttributes = sAttributes + XMLAttribute("StaffName", StaffName, False)
      If (Len(AwardName) > 0) Then sAttributes = sAttributes + XMLAttribute("AwardName", AwardName, False)
      If (Len(PayDate) > 0) And (PayDate <> "0") Then sAttributes = sAttributes + XMLAttribute("PayDate", Format$(PayDate, "m-d-yy h:mm am/pm"), False)
      If (Len(PayType) > 0) Then sAttributes = sAttributes + XMLAttribute("PayType", PayType, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Total) > 0) Then sAttributes = sAttributes + XMLAttribute("Total", Format$(Total, "currency"), False)
      If (Len(Amount) > 0) Then sAttributes = sAttributes + XMLAttribute("Amount", Format$(Amount, "currency"), False)
      If (Len(Merchant) > 0) Then sAttributes = sAttributes + XMLAttribute("Merchant", Format$(Merchant, "#,###.00##"), False)
      If (Len(Cashback) > 0) Then sAttributes = sAttributes + XMLAttribute("Cashback", Format$(Cashback, "#,###.00##"), False)
      If (Len(Fee) > 0) Then sAttributes = sAttributes + XMLAttribute("Fee", Format$(Fee, "#,###.00##"), False)
      If (Len(PayCoins) > 0) Then sAttributes = sAttributes + XMLAttribute("PayCoins", Format$(PayCoins/100000000, "##,###.00000000"), False)
      If (Len(PayRate) > 0) Then sAttributes = sAttributes + XMLAttribute("PayRate", Format$(PayRate, "##,###.0000"), False)
      If (Len(PaidCoins) > 0) Then sAttributes = sAttributes + XMLAttribute("PaidCoins", Format$(PaidCoins/100000000, "##,###.00000000"), False)
      If (Len(Reference) > 0) Then sAttributes = sAttributes + XMLAttribute("Reference", Reference, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Notes) > 0) Then sAttributes = sAttributes + XMLAttribute("Notes", Notes, False)
      If (Len(Ticket) > 0) Then sAttributes = sAttributes + XMLAttribute("Ticket", Ticket, False)
      If (Len(CommStatus) > 0) Then sAttributes = sAttributes + XMLAttribute("CommStatus", CommStatus, False)
      If (Len(CommDate) > 0) And (CommDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CommDate", Format$(CommDate, "m/d/yyyy"), False)
      If (Len(CoinStatus) > 0) Then sAttributes = sAttributes + XMLAttribute("CoinStatus", CoinStatus, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

