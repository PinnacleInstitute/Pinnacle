Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "SalesStep") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(AutoStep) Then
         sChildren = sChildren + EnumItems(7808).XML("ptsAutoSteps", AutoStep)
      Else
         sChildren = sChildren + EnumItems (7808).XML("ptsAutoSteps")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(SalesStepID) > 0) Then sAttributes = sAttributes + XMLAttribute("SalesStepID", SalesStepID, False)
      If (Len(SalesCampaignID) > 0) Then sAttributes = sAttributes + XMLAttribute("SalesCampaignID", SalesCampaignID, False)
      If (Len(EmailID) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailID", EmailID, False)
      If (Len(SalesStepName) > 0) Then sAttributes = sAttributes + XMLAttribute("SalesStepName", SalesStepName, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(AutoStep) > 0) Then sAttributes = sAttributes + XMLAttribute("AutoStep", AutoStep, False)
      If (Len(NextStep) > 0) Then sAttributes = sAttributes + XMLAttribute("NextStep", NextStep, False)
      If (Len(Delay) > 0) Then sAttributes = sAttributes + XMLAttribute("Delay", Delay, False)
      If (Len(DateNo) > 0) Then sAttributes = sAttributes + XMLAttribute("DateNo", DateNo, False)
      If (Len(IsBoard) > 0) Then sAttributes = sAttributes + XMLAttribute("IsBoard", IsBoard, False)
      If (Len(BoardRate) > 0) Then sAttributes = sAttributes + XMLAttribute("BoardRate", Format$(BoardRate, "###,###.000"), False)
      If (Len(Length) > 0) Then sAttributes = sAttributes + XMLAttribute("Length", Length, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(Data) > 0) And (Data <> "0") Then sAttributes = sAttributes + XMLAttribute("Data", Format$(Data, "m/d/yyyy"), False)
      If (Len(Count) > 0) Then sAttributes = sAttributes + XMLAttribute("Count", Count, False)
      If (Len(Potential) > 0) Then sAttributes = sAttributes + XMLAttribute("Potential", Format$(Potential, "currency"), False)
      If (Len(Current) > 0) Then sAttributes = sAttributes + XMLAttribute("Current", Format$(Current, "currency"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

