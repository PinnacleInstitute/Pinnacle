Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "EmailList") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(SourceType) Then
         sChildren = sChildren + EnumItems(8911).XML("ptsSourceTypes", SourceType)
      Else
         sChildren = sChildren + EnumItems (8911).XML("ptsSourceTypes")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(EmailListID) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailListID", EmailListID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(EmailSourceID) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailSourceID", EmailSourceID, False)
      If (Len(EmailListName) > 0) Then sAttributes = sAttributes + XMLAttribute("EmailListName", EmailListName, False)
      If (Len(SourceType) > 0) Then sAttributes = sAttributes + XMLAttribute("SourceType", SourceType, False)
      If (Len(CustomID) > 0) Then sAttributes = sAttributes + XMLAttribute("CustomID", CustomID, False)
      If (Len(Param1) > 0) Then sAttributes = sAttributes + XMLAttribute("Param1", Param1, False)
      If (Len(Param2) > 0) Then sAttributes = sAttributes + XMLAttribute("Param2", Param2, False)
      If (Len(Param3) > 0) Then sAttributes = sAttributes + XMLAttribute("Param3", Param3, False)
      If (Len(Param4) > 0) Then sAttributes = sAttributes + XMLAttribute("Param4", Param4, False)
      If (Len(Param5) > 0) Then sAttributes = sAttributes + XMLAttribute("Param5", Param5, False)
      If (Len(Unsubscribe) > 0) Then sAttributes = sAttributes + XMLAttribute("Unsubscribe", Unsubscribe, False)
      If (Len(Query) > 0) Then sAttributes = sAttributes + XMLAttribute("Query", Query, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

