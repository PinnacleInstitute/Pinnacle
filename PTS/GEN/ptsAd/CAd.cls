Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Ad") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(14306).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (14306).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(AdID) > 0) Then sAttributes = sAttributes + XMLAttribute("AdID", AdID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(MemberName) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberName", MemberName, False)
      If (Len(AdName) > 0) Then sAttributes = sAttributes + XMLAttribute("AdName", AdName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Msg) > 0) Then sChildren = sChildren + XMLElement("Msg", "", "<!-- " + Replace(Msg, "--", "- ") + " -->", "")
      If (Len(Placement) > 0) Then sAttributes = sAttributes + XMLAttribute("Placement", Placement, False)
      If (Len(RefID) > 0) Then sAttributes = sAttributes + XMLAttribute("RefID", RefID, False)
      If (Len(Priority) > 0) Then sAttributes = sAttributes + XMLAttribute("Priority", Priority, False)
      If (Len(POrder) > 0) Then sAttributes = sAttributes + XMLAttribute("POrder", POrder, False)
      If (Len(Zip) > 0) Then sAttributes = sAttributes + XMLAttribute("Zip", Zip, False)
      If (Len(City) > 0) Then sAttributes = sAttributes + XMLAttribute("City", City, False)
      If (Len(MTA) > 0) Then sAttributes = sAttributes + XMLAttribute("MTA", MTA, False)
      If (Len(State) > 0) Then sAttributes = sAttributes + XMLAttribute("State", State, False)
      If (Len(StartAge) > 0) Then sAttributes = sAttributes + XMLAttribute("StartAge", StartAge, False)
      If (Len(EndAge) > 0) Then sAttributes = sAttributes + XMLAttribute("EndAge", EndAge, False)
      If (Len(StartDate) > 0) And (StartDate <> "0") Then sAttributes = sAttributes + XMLAttribute("StartDate", Format$(StartDate, "m/d/yyyy"), False)
      If (Len(EndDate) > 0) And (EndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EndDate", Format$(EndDate, "m/d/yyyy"), False)
      If (Len(MaxPlace) > 0) Then sAttributes = sAttributes + XMLAttribute("MaxPlace", MaxPlace, False)
      If (Len(Places) > 0) Then sAttributes = sAttributes + XMLAttribute("Places", Places, False)
      If (Len(Clicks) > 0) Then sAttributes = sAttributes + XMLAttribute("Clicks", Clicks, False)
      If (Len(Rotation) > 0) Then sAttributes = sAttributes + XMLAttribute("Rotation", Rotation, False)
      If (Len(Weight) > 0) Then sAttributes = sAttributes + XMLAttribute("Weight", Weight, False)
      If (Len(AdTrackID) > 0) Then sAttributes = sAttributes + XMLAttribute("AdTrackID", AdTrackID, False)
      If (Len(ClickRate) > 0) Then sAttributes = sAttributes + XMLAttribute("ClickRate", Format$(ClickRate, "##0.00%"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

