Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "HomeTax") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(TaxRate) Then
         sChildren = sChildren + EnumItems(10403).XML("ptsTaxRates", TaxRate)
      Else
         sChildren = sChildren + EnumItems (10403).XML("ptsTaxRates")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(HomeTaxID) > 0) Then sAttributes = sAttributes + XMLAttribute("HomeTaxID", HomeTaxID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(TaxRate) > 0) Then sAttributes = sAttributes + XMLAttribute("TaxRate", TaxRate, False)
      If (Len(Office) > 0) Then sAttributes = sAttributes + XMLAttribute("Office", Office, False)
      If (Len(Miles) > 0) Then sAttributes = sAttributes + XMLAttribute("Miles", Miles, False)
      If (Len(Home) > 0) Then sAttributes = sAttributes + XMLAttribute("Home", Format$(Home, "currency"), False)
      If (Len(Meetings) > 0) Then sAttributes = sAttributes + XMLAttribute("Meetings", Format$(Meetings, "currency"), False)
      If (Len(Meals) > 0) Then sAttributes = sAttributes + XMLAttribute("Meals", Format$(Meals, "currency"), False)
      If (Len(Supplies) > 0) Then sAttributes = sAttributes + XMLAttribute("Supplies", Format$(Supplies, "currency"), False)
      If (Len(Gifts) > 0) Then sAttributes = sAttributes + XMLAttribute("Gifts", Format$(Gifts, "currency"), False)
      If (Len(Materials) > 0) Then sAttributes = sAttributes + XMLAttribute("Materials", Format$(Materials, "currency"), False)
      If (Len(Phones) > 0) Then sAttributes = sAttributes + XMLAttribute("Phones", Format$(Phones, "currency"), False)
      If (Len(Internet) > 0) Then sAttributes = sAttributes + XMLAttribute("Internet", Format$(Internet, "currency"), False)
      If (Len(Kids) > 0) Then sAttributes = sAttributes + XMLAttribute("Kids", Format$(Kids, "currency"), False)
      If (Len(Subscriptions) > 0) Then sAttributes = sAttributes + XMLAttribute("Subscriptions", Format$(Subscriptions, "currency"), False)
      If (Len(Entertainment) > 0) Then sAttributes = sAttributes + XMLAttribute("Entertainment", Format$(Entertainment, "currency"), False)
      If (Len(Activities) > 0) Then sAttributes = sAttributes + XMLAttribute("Activities", Format$(Activities, "currency"), False)
      If (Len(Events) > 0) Then sAttributes = sAttributes + XMLAttribute("Events", Format$(Events, "currency"), False)
      If (Len(Medical) > 0) Then sAttributes = sAttributes + XMLAttribute("Medical", Format$(Medical, "currency"), False)
      If (Len(Recreational) > 0) Then sAttributes = sAttributes + XMLAttribute("Recreational", Format$(Recreational, "currency"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

