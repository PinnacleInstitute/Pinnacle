Public Property Get EnumItems( _ 
   ByVal bvType As ptsPrepaidEnumConstants) As wtSystem.CEnumItems
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns an enumerated list of items.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsPrepaidEnumPayType
         oEnums.Add cptsPrepaidPayTypeCredit, "Credit"
         oEnums.Add cptsPrepaidPayTypeDebit, "Debit"
      Case cptsPrepaidEnumBonus
         oEnums.Add cptsPrepaidBonusNo, "No"
         oEnums.Add cptsPrepaidBonusYes, "Yes"
         oEnums.Add cptsPrepaidBonusNone, "None"
         oEnums.Add cptsPrepaidBonusReclaimed, "Reclaimed"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

