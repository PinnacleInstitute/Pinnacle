Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Calendar") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Layout) Then
         sChildren = sChildren + EnumItems(4707).XML("ptsLayouts", Layout)
      Else
         sChildren = sChildren + EnumItems (4707).XML("ptsLayouts")
      End If

      If IsNumeric(Timezone) Then
         sChildren = sChildren + EnumItems(4722).XML("ptsTimezones", Timezone)
      Else
         sChildren = sChildren + EnumItems (4722).XML("ptsTimezones")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(CalendarID) > 0) Then sAttributes = sAttributes + XMLAttribute("CalendarID", CalendarID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(CalendarName) > 0) Then sAttributes = sAttributes + XMLAttribute("CalendarName", CalendarName, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Layout) > 0) Then sAttributes = sAttributes + XMLAttribute("Layout", Layout, False)
      If (Len(IsPrivate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPrivate", IsPrivate, False)
      If (Len(IsAppt) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAppt", IsAppt, False)
      If (Len(IsClass) > 0) Then sAttributes = sAttributes + XMLAttribute("IsClass", IsClass, False)
      If (Len(IsAssess) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAssess", IsAssess, False)
      If (Len(IsGoal) > 0) Then sAttributes = sAttributes + XMLAttribute("IsGoal", IsGoal, False)
      If (Len(IsProject) > 0) Then sAttributes = sAttributes + XMLAttribute("IsProject", IsProject, False)
      If (Len(IsTask) > 0) Then sAttributes = sAttributes + XMLAttribute("IsTask", IsTask, False)
      If (Len(IsSales) > 0) Then sAttributes = sAttributes + XMLAttribute("IsSales", IsSales, False)
      If (Len(IsActivities) > 0) Then sAttributes = sAttributes + XMLAttribute("IsActivities", IsActivities, False)
      If (Len(IsEvents) > 0) Then sAttributes = sAttributes + XMLAttribute("IsEvents", IsEvents, False)
      If (Len(IsService) > 0) Then sAttributes = sAttributes + XMLAttribute("IsService", IsService, False)
      If (Len(IsLead) > 0) Then sAttributes = sAttributes + XMLAttribute("IsLead", IsLead, False)
      If (Len(Timezone) > 0) Then sAttributes = sAttributes + XMLAttribute("Timezone", Timezone, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

