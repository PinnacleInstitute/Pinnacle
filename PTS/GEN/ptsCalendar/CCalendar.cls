   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCalendarEnumLayout
         oEnums.Add cptsCalendarLayoutDay, "Day"
         oEnums.Add cptsCalendarLayoutWeek, "Week"
         oEnums.Add cptsCalendarLayoutMonth, "Month"
         oEnums.Add cptsCalendarLayoutList, "List"
      Case cptsCalendarEnumTimezone
         oEnums.Add cptsCalendarTimezoneGMTm4, "GMTm4"
