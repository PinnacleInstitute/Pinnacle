         oEnums.Add cptsCalendarTimezoneGMTm8, "GMTm8"
         oEnums.Add cptsCalendarTimezoneGMTm9, "GMTm9"
         oEnums.Add cptsCalendarTimezoneGMTm10, "GMTm10"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

