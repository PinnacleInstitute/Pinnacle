Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Promo") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(15409).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (15409).XML("ptsStatuss")
      End If

      If IsNumeric(TargetArea) Then
         sChildren = sChildren + EnumItems(15410).XML("ptsTargetAreas", TargetArea)
      Else
         sChildren = sChildren + EnumItems (15410).XML("ptsTargetAreas")
      End If

      If IsNumeric(TargetType) Then
         sChildren = sChildren + EnumItems(15411).XML("ptsTargetTypes", TargetType)
      Else
         sChildren = sChildren + EnumItems (15411).XML("ptsTargetTypes")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(PromoID) > 0) Then sAttributes = sAttributes + XMLAttribute("PromoID", PromoID, False)
      If (Len(MerchantID) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantID", MerchantID, False)
      If (Len(CountryID) > 0) Then sAttributes = sAttributes + XMLAttribute("CountryID", CountryID, False)
      If (Len(PromoName) > 0) Then sAttributes = sAttributes + XMLAttribute("PromoName", PromoName, False)
      If (Len(FromEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("FromEmail", FromEmail, False)
      If (Len(Subject) > 0) Then sAttributes = sAttributes + XMLAttribute("Subject", Subject, False)
      If (Len(Message) > 0) Then sAttributes = sAttributes + XMLAttribute("Message", Message, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(TargetArea) > 0) Then sAttributes = sAttributes + XMLAttribute("TargetArea", TargetArea, False)
      If (Len(TargetType) > 0) Then sAttributes = sAttributes + XMLAttribute("TargetType", TargetType, False)
      If (Len(TargetDays) > 0) Then sAttributes = sAttributes + XMLAttribute("TargetDays", TargetDays, False)
      If (Len(Target) > 0) Then sAttributes = sAttributes + XMLAttribute("Target", Target, False)
      If (Len(StartDate) > 0) And (StartDate <> "0") Then sAttributes = sAttributes + XMLAttribute("StartDate", Format$(StartDate, "m-d-yy h:mm am/pm"), False)
      If (Len(EndDate) > 0) And (EndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EndDate", Format$(EndDate, "m-d-yy h:mm am/pm"), False)
      If (Len(SendDate) > 0) And (SendDate <> "0") Then sAttributes = sAttributes + XMLAttribute("SendDate", Format$(SendDate, "m/d/yyyy"), False)
      If (Len(Msgs) > 0) Then sAttributes = sAttributes + XMLAttribute("Msgs", Msgs, False)
      If (Len(TestEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("TestEmail", TestEmail, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

