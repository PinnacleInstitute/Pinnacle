Public Property Get EnumItems( _ 
   ByVal bvType As ptsPromoEnumConstants) As wtSystem.CEnumItems
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns an enumerated list of items.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsPromoEnumStatus
         oEnums.Add cptsPromoStatusCompose, "Compose"
         oEnums.Add cptsPromoStatusReady, "Ready"
         oEnums.Add cptsPromoStatusSent, "Sent"
         oEnums.Add cptsPromoStatusCancelled, "Cancelled"
      Case cptsPromoEnumTargetArea
         oEnums.Add cptsPromoTargetAreaZip, "Zip"
         oEnums.Add cptsPromoTargetAreaCity, "City"
         oEnums.Add cptsPromoTargetAreaState, "State"
      Case cptsPromoEnumTargetType
         oEnums.Add cptsPromoTargetTypeTargetType1, "TargetType1"
         oEnums.Add cptsPromoTargetTypeTargetType2, "TargetType2"
         oEnums.Add cptsPromoTargetTypeTargetType3, "TargetType3"
         oEnums.Add cptsPromoTargetTypeTargetType4, "TargetType4"
         oEnums.Add cptsPromoTargetTypeTargetType5, "TargetType5"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

