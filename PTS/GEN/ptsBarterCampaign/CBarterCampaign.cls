Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "BarterCampaign") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(17511).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (17511).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(BarterCampaignID) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterCampaignID", BarterCampaignID, False)
      If (Len(ConsumerID) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerID", ConsumerID, False)
      If (Len(BarterAdID) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterAdID", BarterAdID, False)
      If (Len(BarterAreaID) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterAreaID", BarterAreaID, False)
      If (Len(BarterCategoryID) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterCategoryID", BarterCategoryID, False)
      If (Len(CampaignName) > 0) Then sAttributes = sAttributes + XMLAttribute("CampaignName", CampaignName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(StartDate) > 0) And (StartDate <> "0") Then sAttributes = sAttributes + XMLAttribute("StartDate", Format$(StartDate, "m/d/yyyy"), False)
      If (Len(EndDate) > 0) And (EndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EndDate", Format$(EndDate, "m/d/yyyy"), False)
      If (Len(IsKeyword) > 0) Then sAttributes = sAttributes + XMLAttribute("IsKeyword", IsKeyword, False)
      If (Len(IsAllCategory) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAllCategory", IsAllCategory, False)
      If (Len(IsMainCategory) > 0) Then sAttributes = sAttributes + XMLAttribute("IsMainCategory", IsMainCategory, False)
      If (Len(IsSubCategory) > 0) Then sAttributes = sAttributes + XMLAttribute("IsSubCategory", IsSubCategory, False)
      If (Len(IsAllLocation) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAllLocation", IsAllLocation, False)
      If (Len(IsCountry) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCountry", IsCountry, False)
      If (Len(IsState) > 0) Then sAttributes = sAttributes + XMLAttribute("IsState", IsState, False)
      If (Len(IsCity) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCity", IsCity, False)
      If (Len(IsArea) > 0) Then sAttributes = sAttributes + XMLAttribute("IsArea", IsArea, False)
      If (Len(Keyword) > 0) Then sAttributes = sAttributes + XMLAttribute("Keyword", Keyword, False)
      If (Len(FT) > 0) Then sAttributes = sAttributes + XMLAttribute("FT", FT, False)
      If (Len(Credits) > 0) Then sAttributes = sAttributes + XMLAttribute("Credits", Format$(Credits, "##,###.00"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

