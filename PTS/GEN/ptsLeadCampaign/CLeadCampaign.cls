Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "LeadCampaign") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(1411).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (1411).XML("ptsStatuss")
      End If

      If IsNumeric(PageType) Then
         sChildren = sChildren + EnumItems(1412).XML("ptsPageTypes", PageType)
      Else
         sChildren = sChildren + EnumItems (1412).XML("ptsPageTypes")
      End If

      If IsNumeric(Entity) Then
         sChildren = sChildren + EnumItems(1424).XML("ptsEntitys", Entity, 0)
      Else
         sChildren = sChildren + EnumItems (1424).XML("ptsEntitys", , 0)
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(LeadCampaignID) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadCampaignID", LeadCampaignID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(SalesCampaignID) > 0) Then sAttributes = sAttributes + XMLAttribute("SalesCampaignID", SalesCampaignID, False)
      If (Len(ProspectTypeID) > 0) Then sAttributes = sAttributes + XMLAttribute("ProspectTypeID", ProspectTypeID, False)
      If (Len(CycleID) > 0) Then sAttributes = sAttributes + XMLAttribute("CycleID", CycleID, False)
      If (Len(NewsLetterID) > 0) Then sAttributes = sAttributes + XMLAttribute("NewsLetterID", NewsLetterID, False)
      If (Len(GroupID) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID", GroupID, False)
      If (Len(FolderID) > 0) Then sAttributes = sAttributes + XMLAttribute("FolderID", FolderID, False)
      If (Len(LeadCampaignName) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadCampaignName", LeadCampaignName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(PageType) > 0) Then sAttributes = sAttributes + XMLAttribute("PageType", PageType, False)
      If (Len(Objective) > 0) Then sAttributes = sAttributes + XMLAttribute("Objective", Objective, False)
      If (Len(Title) > 0) Then sAttributes = sAttributes + XMLAttribute("Title", Title, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Keywords) > 0) Then sAttributes = sAttributes + XMLAttribute("Keywords", Keywords, False)
      If (Len(IsMember) > 0) Then sAttributes = sAttributes + XMLAttribute("IsMember", IsMember, False)
      If (Len(IsAffiliate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAffiliate", IsAffiliate, False)
      If (Len(Cycle) > 0) Then sAttributes = sAttributes + XMLAttribute("Cycle", Cycle, False)
      If (Len(CSS) > 0) Then sAttributes = sAttributes + XMLAttribute("CSS", CSS, False)
      If (Len(NoEdit) > 0) Then sAttributes = sAttributes + XMLAttribute("NoEdit", NoEdit, False)
      If (Len(Page) > 0) Then sAttributes = sAttributes + XMLAttribute("Page", Page, False)
      If (Len(Image) > 0) Then sAttributes = sAttributes + XMLAttribute("Image", Image, False)
      If (Len(Entity) > 0) Then sAttributes = sAttributes + XMLAttribute("Entity", Entity, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(GroupID1) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID1", GroupID1, False)
      If (Len(GroupID2) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID2", GroupID2, False)
      If (Len(GroupID3) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID3", GroupID3, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

