Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Comment") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(14612).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (14612).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(CommentID) > 0) Then sAttributes = sAttributes + XMLAttribute("CommentID", CommentID, False)
      If (Len(OwnerType) > 0) Then sAttributes = sAttributes + XMLAttribute("OwnerType", OwnerType, False)
      If (Len(OwnerID) > 0) Then sAttributes = sAttributes + XMLAttribute("OwnerID", OwnerID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(ReplyID) > 0) Then sAttributes = sAttributes + XMLAttribute("ReplyID", ReplyID, False)
      If (Len(CommentDate) > 0) And (CommentDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CommentDate", Format$(CommentDate, "m-d-yy h:mmam/pm"), False)
      If (Len(Msg) > 0) Then sChildren = sChildren + XMLElement("Msg", "", "<!-- " + Replace(Msg, "--", "- ") + " -->", "")
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Likes) > 0) Then sAttributes = sAttributes + XMLAttribute("Likes", Likes, False)
      If (Len(Dislikes) > 0) Then sAttributes = sAttributes + XMLAttribute("Dislikes", Dislikes, False)
      If (Len(Bads) > 0) Then sAttributes = sAttributes + XMLAttribute("Bads", Bads, False)
      If (Len(Favorites) > 0) Then sAttributes = sAttributes + XMLAttribute("Favorites", Favorites, False)
      If (Len(MemberName) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberName", MemberName, False)
      If (Len(ReplyName) > 0) Then sAttributes = sAttributes + XMLAttribute("ReplyName", ReplyName, False)
      If (Len(Stat) > 0) Then sAttributes = sAttributes + XMLAttribute("Stat", Stat, False)
      If (Len(Num) > 0) Then sAttributes = sAttributes + XMLAttribute("Num", Num, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

