   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsEmaileeEnumStatus
         oEnums.Add cptsEmaileeStatusActive, "Active"
         oEnums.Add cptsEmaileeStatusInActive, "InActive"
         oEnums.Add cptsEmaileeStatusUnsubscribed, "Unsubscribed"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

