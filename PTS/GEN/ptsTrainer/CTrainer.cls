         oEnums.Add cptsTrainerStatusInActive, "InActive"
      Case cptsTrainerEnumTier
         oEnums.Add cptsTrainerTierTier1, "Tier1"
         oEnums.Add cptsTrainerTierTier2, "Tier2"
         oEnums.Add cptsTrainerTierTier3, "Tier3"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
