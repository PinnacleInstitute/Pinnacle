   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsTrainerEnumUserGroup
         oEnums.Add cptsTrainerUserGroupTrainer, "Trainer"
      Case cptsTrainerEnumUserStatus
         oEnums.Add cptsTrainerUserStatusActive, "Active"
         oEnums.Add cptsTrainerUserStatusInActive, "InActive"
