Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Task") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(7422).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (7422).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(TaskID) > 0) Then sAttributes = sAttributes + XMLAttribute("TaskID", TaskID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(ParentID) > 0) Then sAttributes = sAttributes + XMLAttribute("ParentID", ParentID, False)
      If (Len(ProjectID) > 0) Then sAttributes = sAttributes + XMLAttribute("ProjectID", ProjectID, False)
      If (Len(DependentID) > 0) Then sAttributes = sAttributes + XMLAttribute("DependentID", DependentID, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(MemberName) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberName", MemberName, False)
      If (Len(ProjectName) > 0) Then sAttributes = sAttributes + XMLAttribute("ProjectName", ProjectName, False)
      If (Len(TaskName) > 0) Then sAttributes = sAttributes + XMLAttribute("TaskName", TaskName, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(IsMilestone) > 0) Then sAttributes = sAttributes + XMLAttribute("IsMilestone", IsMilestone, False)
      If (Len(EstStartDate) > 0) And (EstStartDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EstStartDate", Format$(EstStartDate, "m/d/yyyy"), False)
      If (Len(ActStartDate) > 0) And (ActStartDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ActStartDate", Format$(ActStartDate, "m/d/yyyy"), False)
      If (Len(VarStartDate) > 0) Then sAttributes = sAttributes + XMLAttribute("VarStartDate", VarStartDate, False)
      If (Len(EstEndDate) > 0) And (EstEndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EstEndDate", Format$(EstEndDate, "m/d/yyyy"), False)
      If (Len(ActEndDate) > 0) And (ActEndDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ActEndDate", Format$(ActEndDate, "m/d/yyyy"), False)
      If (Len(VarEndDate) > 0) Then sAttributes = sAttributes + XMLAttribute("VarEndDate", VarEndDate, False)
      If (Len(EstCost) > 0) Then sAttributes = sAttributes + XMLAttribute("EstCost", Format$(EstCost, "currency"), False)
      If (Len(TotCost) > 0) Then sAttributes = sAttributes + XMLAttribute("TotCost", Format$(TotCost, "currency"), False)
      If (Len(VarCost) > 0) Then sAttributes = sAttributes + XMLAttribute("VarCost", Format$(VarCost, "currency"), False)
      If (Len(Cost) > 0) Then sAttributes = sAttributes + XMLAttribute("Cost", Format$(Cost, "currency"), False)
      If (Len(Hrs) > 0) Then sAttributes = sAttributes + XMLAttribute("Hrs", Format$(Hrs, "###,##0.00"), False)
      If (Len(TotHrs) > 0) Then sAttributes = sAttributes + XMLAttribute("TotHrs", Format$(TotHrs, "###,##0.00"), False)
      If (Len(FromDate) > 0) And (FromDate <> "0") Then sAttributes = sAttributes + XMLAttribute("FromDate", Format$(FromDate, "m/d/yyyy"), False)
      If (Len(ToDate) > 0) And (ToDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ToDate", Format$(ToDate, "m/d/yyyy"), False)
      If (Len(Notes) > 0) Then sAttributes = sAttributes + XMLAttribute("Notes", Notes, False)
      If (Len(Documents) > 0) Then sAttributes = sAttributes + XMLAttribute("Documents", Documents, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(Secure) > 0) Then sAttributes = sAttributes + XMLAttribute("Secure", Secure, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(FromEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("FromEmail", FromEmail, False)
      If (Len(IsMsg) > 0) Then sAttributes = sAttributes + XMLAttribute("IsMsg", IsMsg, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

