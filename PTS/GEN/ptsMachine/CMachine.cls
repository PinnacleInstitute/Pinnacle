Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Machine") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(10718).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (10718).XML("ptsStatuss")
      End If

      If IsNumeric(Service) Then
         sChildren = sChildren + EnumItems(10719).XML("ptsServices", Service)
      Else
         sChildren = sChildren + EnumItems (10719).XML("ptsServices")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(MachineID) > 0) Then sAttributes = sAttributes + XMLAttribute("MachineID", MachineID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(MemberNameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberNameFirst", MemberNameFirst, False)
      If (Len(MemberNameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberNameLast", MemberNameLast, False)
      If (Len(MemberEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberEmail", MemberEmail, False)
      If (Len(LiveDriveID) > 0) Then sAttributes = sAttributes + XMLAttribute("LiveDriveID", LiveDriveID, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(MachineName) > 0) Then sAttributes = sAttributes + XMLAttribute("MachineName", MachineName, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(Password) > 0) Then sAttributes = sAttributes + XMLAttribute("Password", Password, False)
      If (Len(WebName) > 0) Then sAttributes = sAttributes + XMLAttribute("WebName", WebName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Service) > 0) Then sAttributes = sAttributes + XMLAttribute("Service", Service, False)
      If (Len(ActiveDate) > 0) And (ActiveDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ActiveDate", Format$(ActiveDate, "mm/dd/yy"), False)
      If (Len(CancelDate) > 0) And (CancelDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CancelDate", Format$(CancelDate, "mm/dd/yy"), False)
      If (Len(RemoveDate) > 0) And (RemoveDate <> "0") Then sAttributes = sAttributes + XMLAttribute("RemoveDate", Format$(RemoveDate, "mm/dd/yy"), False)
      If (Len(BackupUsed) > 0) Then sAttributes = sAttributes + XMLAttribute("BackupUsed", BackupUsed, False)
      If (Len(BackupCapacity) > 0) Then sAttributes = sAttributes + XMLAttribute("BackupCapacity", BackupCapacity, False)
      If (Len(BriefcaseUsed) > 0) Then sAttributes = sAttributes + XMLAttribute("BriefcaseUsed", BriefcaseUsed, False)
      If (Len(BriefcaseCapacity) > 0) Then sAttributes = sAttributes + XMLAttribute("BriefcaseCapacity", BriefcaseCapacity, False)
      If (Len(Qty) > 0) Then sAttributes = sAttributes + XMLAttribute("Qty", Qty, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

