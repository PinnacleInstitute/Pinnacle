Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Merchant") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(15023).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (15023).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(MerchantID) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantID", MerchantID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(BillingID) > 0) Then sAttributes = sAttributes + XMLAttribute("BillingID", BillingID, False)
      If (Len(PayoutID) > 0) Then sAttributes = sAttributes + XMLAttribute("PayoutID", PayoutID, False)
      If (Len(CountryID) > 0) Then sAttributes = sAttributes + XMLAttribute("CountryID", CountryID, False)
      If (Len(SweepID) > 0) Then sAttributes = sAttributes + XMLAttribute("SweepID", SweepID, False)
      If (Len(MemberNameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberNameLast", MemberNameLast, False)
      If (Len(MemberNameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberNameFirst", MemberNameFirst, False)
      If (Len(MemberName) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberName", MemberName, False)
      If (Len(CountryName) > 0) Then sAttributes = sAttributes + XMLAttribute("CountryName", CountryName, False)
      If (Len(MerchantName) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantName", MerchantName, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(Email) > 0) Then sAttributes = sAttributes + XMLAttribute("Email", Email, False)
      If (Len(Email2) > 0) Then sAttributes = sAttributes + XMLAttribute("Email2", Email2, False)
      If (Len(Email3) > 0) Then sAttributes = sAttributes + XMLAttribute("Email3", Email3, False)
      If (Len(Phone) > 0) Then sAttributes = sAttributes + XMLAttribute("Phone", Phone, False)
      If (Len(Phone2) > 0) Then sAttributes = sAttributes + XMLAttribute("Phone2", Phone2, False)
      If (Len(Password) > 0) Then sAttributes = sAttributes + XMLAttribute("Password", Password, False)
      If (Len(Password2) > 0) Then sAttributes = sAttributes + XMLAttribute("Password2", Password2, False)
      If (Len(Password3) > 0) Then sAttributes = sAttributes + XMLAttribute("Password3", Password3, False)
      If (Len(Password4) > 0) Then sAttributes = sAttributes + XMLAttribute("Password4", Password4, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Street1) > 0) Then sAttributes = sAttributes + XMLAttribute("Street1", Street1, False)
      If (Len(Street2) > 0) Then sAttributes = sAttributes + XMLAttribute("Street2", Street2, False)
      If (Len(City) > 0) Then sAttributes = sAttributes + XMLAttribute("City", City, False)
      If (Len(State) > 0) Then sAttributes = sAttributes + XMLAttribute("State", State, False)
      If (Len(Zip) > 0) Then sAttributes = sAttributes + XMLAttribute("Zip", Zip, False)
      If (Len(Referrals) > 0) Then sAttributes = sAttributes + XMLAttribute("Referrals", Referrals, False)
      If (Len(Referrals2) > 0) Then sAttributes = sAttributes + XMLAttribute("Referrals2", Referrals2, False)
      If (Len(VisitDate) > 0) And (VisitDate <> "0") Then sAttributes = sAttributes + XMLAttribute("VisitDate", Format$(VisitDate, "m-d-yy h:mm am/pm"), False)
      If (Len(IsOrg) > 0) Then sAttributes = sAttributes + XMLAttribute("IsOrg", IsOrg, False)
      If (Len(IsAwards) > 0) Then sAttributes = sAttributes + XMLAttribute("IsAwards", IsAwards, False)
      If (Len(EnrollDate) > 0) And (EnrollDate <> "0") Then sAttributes = sAttributes + XMLAttribute("EnrollDate", Format$(EnrollDate, "m-d-yy h:mm am/pm"), False)
      If (Len(BillDate) > 0) And (BillDate <> "0") Then sAttributes = sAttributes + XMLAttribute("BillDate", Format$(BillDate, "m/d/yyyy"), False)
      If (Len(BillDays) > 0) Then sAttributes = sAttributes + XMLAttribute("BillDays", BillDays, False)
      If (Len(Image) > 0) Then sAttributes = sAttributes + XMLAttribute("Image", Image, False)
      If (Len(Description) > 0) Then sChildren = sChildren + XMLElement("Description", "", "<!-- " + Replace(Description, "--", "- ") + " -->", "")
      If (Len(Terms) > 0) Then sAttributes = sAttributes + XMLAttribute("Terms", Terms, False)
      If (Len(Options) > 0) Then sAttributes = sAttributes + XMLAttribute("Options", Options, False)
      If (Len(StoreOptions) > 0) Then sAttributes = sAttributes + XMLAttribute("StoreOptions", StoreOptions, False)
      If (Len(Colors) > 0) Then sAttributes = sAttributes + XMLAttribute("Colors", Colors, False)
      If (Len(Rating) > 0) Then sAttributes = sAttributes + XMLAttribute("Rating", Rating, False)
      If (Len(CurrencyCode) > 0) Then sAttributes = sAttributes + XMLAttribute("CurrencyCode", CurrencyCode, False)
      If (Len(Processor) > 0) Then sAttributes = sAttributes + XMLAttribute("Processor", Processor, False)
      If (Len(Payment) > 0) Then sAttributes = sAttributes + XMLAttribute("Payment", Payment, False)
      If (Len(UserKey) > 0) Then sAttributes = sAttributes + XMLAttribute("UserKey", UserKey, False)
      If (Len(UserKey3) > 0) Then sAttributes = sAttributes + XMLAttribute("UserKey3", UserKey3, False)
      If (Len(UserKey4) > 0) Then sAttributes = sAttributes + XMLAttribute("UserKey4", UserKey4, False)
      If (Len(UserCode) > 0) Then sAttributes = sAttributes + XMLAttribute("UserCode", UserCode, False)
      If (Len(Access) > 0) Then sAttributes = sAttributes + XMLAttribute("Access", Access, False)
      If (Len(PromoLimit) > 0) Then sAttributes = sAttributes + XMLAttribute("PromoLimit", PromoLimit, False)
      If (Len(SweepRate) > 0) Then sAttributes = sAttributes + XMLAttribute("SweepRate", SweepRate, False)
      If (Len(TimeZone) > 0) Then sAttributes = sAttributes + XMLAttribute("TimeZone", TimeZone, False)
      If (Len(GeoCode) > 0) Then sAttributes = sAttributes + XMLAttribute("GeoCode", GeoCode, False)
      If (Len(ReferRate) > 0) Then sAttributes = sAttributes + XMLAttribute("ReferRate", ReferRate, False)
      If (Len(Target) > 0) Then sAttributes = sAttributes + XMLAttribute("Target", Target, False)
      If (Len(RewardAmt) > 0) Then sAttributes = sAttributes + XMLAttribute("RewardAmt", Format$(RewardAmt, "##,###.00"), False)
      If (Len(RewardDesc) > 0) Then sAttributes = sAttributes + XMLAttribute("RewardDesc", RewardDesc, False)
      If (Len(RewardCap) > 0) Then sAttributes = sAttributes + XMLAttribute("RewardCap", Format$(RewardCap, "currency"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

