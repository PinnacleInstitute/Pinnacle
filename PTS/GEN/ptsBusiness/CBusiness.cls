End Sub

Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Business") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(BusinessID) > 0) Then sAttributes = sAttributes + XMLAttribute("BusinessID", BusinessID, False)
      If (Len(Install) > 0) Then sAttributes = sAttributes + XMLAttribute("Install", Install, False)
      If (Len(BusinessName) > 0) Then sAttributes = sAttributes + XMLAttribute("BusinessName", BusinessName, False)
      If (Len(SystemEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("SystemEmail", SystemEmail, False)
      If (Len(CustomerEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("CustomerEmail", CustomerEmail, False)
      If (Len(TrainerEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("TrainerEmail", TrainerEmail, False)
      If (Len(FinanceEmail) > 0) Then sAttributes = sAttributes + XMLAttribute("FinanceEmail", FinanceEmail, False)
      If (Len(Street) > 0) Then sAttributes = sAttributes + XMLAttribute("Street", Street, False)
      If (Len(Unit) > 0) Then sAttributes = sAttributes + XMLAttribute("Unit", Unit, False)
      If (Len(City) > 0) Then sAttributes = sAttributes + XMLAttribute("City", City, False)
      If (Len(State) > 0) Then sAttributes = sAttributes + XMLAttribute("State", State, False)
      If (Len(Zip) > 0) Then sAttributes = sAttributes + XMLAttribute("Zip", Zip, False)
      If (Len(Country) > 0) Then sAttributes = sAttributes + XMLAttribute("Country", Country, False)
      If (Len(Phone) > 0) Then sAttributes = sAttributes + XMLAttribute("Phone", Phone, False)
      If (Len(Fax) > 0) Then sAttributes = sAttributes + XMLAttribute("Fax", Fax, False)
      If (Len(WebSite) > 0) Then sAttributes = sAttributes + XMLAttribute("WebSite", WebSite, False)
      If (Len(TaxRate) > 0) Then sAttributes = sAttributes + XMLAttribute("TaxRate", Format$(TaxRate, "###,##0.0000"), False)
      If (Len(CardProcessor) > 0) Then sAttributes = sAttributes + XMLAttribute("CardProcessor", CardProcessor, False)
      If (Len(CheckProcessor) > 0) Then sAttributes = sAttributes + XMLAttribute("CheckProcessor", CheckProcessor, False)
      If (Len(CardAcct) > 0) Then sAttributes = sAttributes + XMLAttribute("CardAcct", CardAcct, False)
      If (Len(CheckAcct) > 0) Then sAttributes = sAttributes + XMLAttribute("CheckAcct", CheckAcct, False)
      If (Len(PayPalAcct) > 0) Then sAttributes = sAttributes + XMLAttribute("PayPalAcct", PayPalAcct, False)
      If (Len(IsNotifyUser) > 0) Then sAttributes = sAttributes + XMLAttribute("IsNotifyUser", IsNotifyUser, False)
      If (Len(PaymentOptions) > 0) Then sAttributes = sAttributes + XMLAttribute("PaymentOptions", PaymentOptions, False)
      If (Len(MiscPay1) > 0) Then sAttributes = sAttributes + XMLAttribute("MiscPay1", MiscPay1, False)
      If (Len(MiscPay2) > 0) Then sAttributes = sAttributes + XMLAttribute("MiscPay2", MiscPay2, False)
      If (Len(MiscPay3) > 0) Then sAttributes = sAttributes + XMLAttribute("MiscPay3", MiscPay3, False)
      If (Len(Languages) > 0) Then sAttributes = sAttributes + XMLAttribute("Languages", Languages, False)
      If (Len(Options1) > 0) Then sAttributes = sAttributes + XMLAttribute("Options1", Options1, False)
      If (Len(Options2) > 0) Then sAttributes = sAttributes + XMLAttribute("Options2", Options2, False)
      If (Len(Options3) > 0) Then sAttributes = sAttributes + XMLAttribute("Options3", Options3, False)
      If (Len(Options4) > 0) Then sAttributes = sAttributes + XMLAttribute("Options4", Options4, False)
      If (Len(Tutorial) > 0) Then sAttributes = sAttributes + XMLAttribute("Tutorial", Tutorial, False)
      If (Len(Timezone) > 0) Then sAttributes = sAttributes + XMLAttribute("Timezone", Timezone, False)
      If (Len(Rpt) > 0) Then sAttributes = sAttributes + XMLAttribute("Rpt", Rpt, False)
      If (Len(R1) > 0) Then sAttributes = sAttributes + XMLAttribute("R1", R1, False)
      If (Len(R2) > 0) Then sAttributes = sAttributes + XMLAttribute("R2", R2, False)
      If (Len(R3) > 0) Then sAttributes = sAttributes + XMLAttribute("R3", R3, False)
      If (Len(R4) > 0) Then sAttributes = sAttributes + XMLAttribute("R4", R4, False)
      If (Len(R5) > 0) Then sAttributes = sAttributes + XMLAttribute("R5", R5, False)
      If (Len(R6) > 0) Then sAttributes = sAttributes + XMLAttribute("R6", R6, False)
      If (Len(R7) > 0) Then sAttributes = sAttributes + XMLAttribute("R7", R7, False)
      If (Len(R8) > 0) Then sAttributes = sAttributes + XMLAttribute("R8", R8, False)
      If (Len(R9) > 0) Then sAttributes = sAttributes + XMLAttribute("R9", R9, False)
      If (Len(R10) > 0) Then sAttributes = sAttributes + XMLAttribute("R10", R10, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

