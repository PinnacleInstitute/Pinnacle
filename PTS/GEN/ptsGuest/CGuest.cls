   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsGuestEnumStatus
         oEnums.Add cptsGuestStatusNotSent, "NotSent"
         oEnums.Add cptsGuestStatusSent, "Sent"
         oEnums.Add cptsGuestStatusAckYes, "AckYes"
         oEnums.Add cptsGuestStatusAckNo, "AckNo"
         oEnums.Add cptsGuestStatusAckMaybe, "AckMaybe"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

