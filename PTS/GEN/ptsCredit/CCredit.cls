   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCreditEnumCreditType
         oEnums.Add cptsCreditCreditTypeCreditType1, "CreditType1"
         oEnums.Add cptsCreditCreditTypeCreditType2, "CreditType2"
         oEnums.Add cptsCreditCreditTypeCreditType3, "CreditType3"
         oEnums.Add cptsCreditCreditTypeCreditType4, "CreditType4"
         oEnums.Add cptsCreditCreditTypeCreditType5, "CreditType5"
      Case cptsCreditEnumStatus
         oEnums.Add cptsCreditStatusPending, "Pending"
         oEnums.Add cptsCreditStatusActive, "Active"
         oEnums.Add cptsCreditStatusInactive, "Inactive"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

