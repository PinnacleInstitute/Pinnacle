Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "News") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(9816).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (9816).XML("ptsStatuss")
      End If

      If IsNumeric(LeadMain) Then
         sChildren = sChildren + EnumItems(9818).XML("ptsLeadMains", LeadMain)
      Else
         sChildren = sChildren + EnumItems (9818).XML("ptsLeadMains")
      End If

      If IsNumeric(LeadTopic) Then
         sChildren = sChildren + EnumItems(9819).XML("ptsLeadTopics", LeadTopic)
      Else
         sChildren = sChildren + EnumItems (9819).XML("ptsLeadTopics")
      End If

      If IsNumeric(UserRole) Then
         sChildren = sChildren + EnumItems(9825).XML("ptsUserRoles", UserRole)
      Else
         sChildren = sChildren + EnumItems (9825).XML("ptsUserRoles")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(NewsID) > 0) Then sAttributes = sAttributes + XMLAttribute("NewsID", NewsID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(AuthUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserID", AuthUserID, False)
      If (Len(NewsTopicID) > 0) Then sAttributes = sAttributes + XMLAttribute("NewsTopicID", NewsTopicID, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(UserName) > 0) Then sAttributes = sAttributes + XMLAttribute("UserName", UserName, False)
      If (Len(NewsTopicName) > 0) Then sAttributes = sAttributes + XMLAttribute("NewsTopicName", NewsTopicName, False)
      If (Len(Title) > 0) Then sAttributes = sAttributes + XMLAttribute("Title", Title, False)
      If (Len(Description) > 0) Then sChildren = sChildren + XMLElement("Description", "", "<!-- " + Replace(Description, "--", "- ") + " -->", "")
      If (Len(CreateDate) > 0) And (CreateDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CreateDate", Format$(CreateDate, "m-d-yy h:mmam/pm"), False)
      If (Len(ActiveDate) > 0) And (ActiveDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ActiveDate", Format$(ActiveDate, "m-d-yy h:mmam/pm"), False)
      If (Len(Image) > 0) Then sAttributes = sAttributes + XMLAttribute("Image", Image, False)
      If (Len(Tags) > 0) Then sAttributes = sAttributes + XMLAttribute("Tags", Tags, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(LeadMain) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadMain", LeadMain, False)
      If (Len(LeadTopic) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadTopic", LeadTopic, False)
      If (Len(IsBreaking) > 0) Then sAttributes = sAttributes + XMLAttribute("IsBreaking", IsBreaking, False)
      If (Len(IsBreaking2) > 0) Then sAttributes = sAttributes + XMLAttribute("IsBreaking2", IsBreaking2, False)
      If (Len(IsStrategic) > 0) Then sAttributes = sAttributes + XMLAttribute("IsStrategic", IsStrategic, False)
      If (Len(IsShare) > 0) Then sAttributes = sAttributes + XMLAttribute("IsShare", IsShare, False)
      If (Len(IsEditorial) > 0) Then sAttributes = sAttributes + XMLAttribute("IsEditorial", IsEditorial, False)
      If (Len(UserRole) > 0) Then sAttributes = sAttributes + XMLAttribute("UserRole", UserRole, False)
      If (Len(FromDate) > 0) And (FromDate <> "0") Then sAttributes = sAttributes + XMLAttribute("FromDate", Format$(FromDate, "m/d/yyyy"), False)
      If (Len(ToDate) > 0) And (ToDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ToDate", Format$(ToDate, "m/d/yyyy"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

