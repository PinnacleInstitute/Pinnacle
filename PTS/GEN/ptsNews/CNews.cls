   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsNewsEnumStatus
         oEnums.Add cptsNewsStatusPending, "Pending"
         oEnums.Add cptsNewsStatusSubmitted, "Submitted"
         oEnums.Add cptsNewsStatusApproved, "Approved"
         oEnums.Add cptsNewsStatusActive, "Active"
         oEnums.Add cptsNewsStatusInActive, "InActive"
         oEnums.Add cptsNewsStatusCancelled, "Cancelled"
      Case cptsNewsEnumLeadMain
         oEnums.Add cptsNewsLeadMainPrimary, "Primary"
         oEnums.Add cptsNewsLeadMainSecondary, "Secondary"
      Case cptsNewsEnumLeadTopic
         oEnums.Add cptsNewsLeadTopicPrimary, "Primary"
         oEnums.Add cptsNewsLeadTopicSecondary, "Secondary"
      Case cptsNewsEnumUserRole
         oEnums.Add cptsNewsUserRoleAuthor, "Author"
         oEnums.Add cptsNewsUserRolePublisher, "Publisher"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

