Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "LeadPage") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oInputOptions As wtSystem.CInputOptions
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(1504).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (1504).XML("ptsStatuss")
      End If

   End If
   If bFilter Then
      Set oInputOptions = New wtSystem.CInputOptions
      oInputOptions.Load Inputs, ""
      sChildren = sChildren + oInputOptions.XML("Inputs")
      Set oInputOptions = Nothing
   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(LeadPageID) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadPageID", LeadPageID, False)
      If (Len(LeadCampaignID) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadCampaignID", LeadCampaignID, False)
      If (Len(LeadPageName) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadPageName", LeadPageName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(IsInput) > 0) Then sAttributes = sAttributes + XMLAttribute("IsInput", IsInput, False)
      If (Len(IsCapture) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCapture", IsCapture, False)
      If (Len(IsProspect) > 0) Then sAttributes = sAttributes + XMLAttribute("IsProspect", IsProspect, False)
      If (Len(IsNext) > 0) Then sAttributes = sAttributes + XMLAttribute("IsNext", IsNext, False)
      If (Len(NextCaption) > 0) Then sAttributes = sAttributes + XMLAttribute("NextCaption", NextCaption, False)
      If (Len(Inputs) > 0) Then sAttributes = sAttributes + XMLAttribute("Inputs", Inputs, False)
      If (Len(Language) > 0) Then sAttributes = sAttributes + XMLAttribute("Language", Language, False)
      If (Len(IsLeadURL) > 0) Then sAttributes = sAttributes + XMLAttribute("IsLeadURL", IsLeadURL, False)
      If (Len(IsRedirectURL) > 0) Then sAttributes = sAttributes + XMLAttribute("IsRedirectURL", IsRedirectURL, False)
      If (Len(LeadURL) > 0) Then sAttributes = sAttributes + XMLAttribute("LeadURL", LeadURL, False)
      If (Len(RedirectURL) > 0) Then sAttributes = sAttributes + XMLAttribute("RedirectURL", RedirectURL, False)
      If (Len(TopCode) > 0) Then sAttributes = sAttributes + XMLAttribute("TopCode", TopCode, False)
      If (Len(ReportFromDate) > 0) And (ReportFromDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ReportFromDate", Format$(ReportFromDate, "m/d/yyyy"), False)
      If (Len(ReportToDate) > 0) And (ReportToDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ReportToDate", Format$(ReportToDate, "m/d/yyyy"), False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(AffiliateID) > 0) Then sAttributes = sAttributes + XMLAttribute("AffiliateID", AffiliateID, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

