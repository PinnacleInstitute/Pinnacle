Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Reward") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(RewardType) Then
         sChildren = sChildren + EnumItems(15212).XML("ptsRewardTypes", RewardType)
      Else
         sChildren = sChildren + EnumItems (15212).XML("ptsRewardTypes")
      End If

      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(15215).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (15215).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(RewardID) > 0) Then sAttributes = sAttributes + XMLAttribute("RewardID", RewardID, False)
      If (Len(MerchantID) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantID", MerchantID, False)
      If (Len(ConsumerID) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerID", ConsumerID, False)
      If (Len(Payment2ID) > 0) Then sAttributes = sAttributes + XMLAttribute("Payment2ID", Payment2ID, False)
      If (Len(AwardID) > 0) Then sAttributes = sAttributes + XMLAttribute("AwardID", AwardID, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(ConsumerName) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerName", ConsumerName, False)
      If (Len(MerchantName) > 0) Then sAttributes = sAttributes + XMLAttribute("MerchantName", MerchantName, False)
      If (Len(AwardName) > 0) Then sAttributes = sAttributes + XMLAttribute("AwardName", AwardName, False)
      If (Len(RewardDate) > 0) And (RewardDate <> "0") Then sAttributes = sAttributes + XMLAttribute("RewardDate", Format$(RewardDate, "m-d-yy h:mm am/pm"), False)
      If (Len(RewardType) > 0) Then sAttributes = sAttributes + XMLAttribute("RewardType", RewardType, False)
      If (Len(Amount) > 0) Then sAttributes = sAttributes + XMLAttribute("Amount", Format$(Amount/100000000, "##,###.00"), False)
      If (Len(Points) > 0) Then sAttributes = sAttributes + XMLAttribute("Points", Format$(Points/100000000, "##,###.00"), False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(HoldDate) > 0) And (HoldDate <> "0") Then sAttributes = sAttributes + XMLAttribute("HoldDate", Format$(HoldDate, "m/d/yyyy"), False)
      If (Len(Reference) > 0) Then sAttributes = sAttributes + XMLAttribute("Reference", Reference, False)
      If (Len(Note) > 0) Then sAttributes = sAttributes + XMLAttribute("Note", Note, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

