   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsPageEnumStatus
         oEnums.Add cptsPageStatusActive, "Active"
         oEnums.Add cptsPageStatusInActive, "InActive"
      Case cptsPageEnumForm
         oEnums.Add cptsPageFormMember, "Member"
         oEnums.Add cptsPageFormProspect, "Prospect"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

