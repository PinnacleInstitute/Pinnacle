Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Page") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(9308).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (9308).XML("ptsStatuss")
      End If

      If IsNumeric(Form) Then
         sChildren = sChildren + EnumItems(9311).XML("ptsForms", Form)
      Else
         sChildren = sChildren + EnumItems (9311).XML("ptsForms")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(PageID) > 0) Then sAttributes = sAttributes + XMLAttribute("PageID", PageID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(PageName) > 0) Then sAttributes = sAttributes + XMLAttribute("PageName", PageName, False)
      If (Len(Category) > 0) Then sAttributes = sAttributes + XMLAttribute("Category", Category, False)
      If (Len(PageType) > 0) Then sAttributes = sAttributes + XMLAttribute("PageType", PageType, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Language) > 0) Then sAttributes = sAttributes + XMLAttribute("Language", Language, False)
      If (Len(IsPrivate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPrivate", IsPrivate, False)
      If (Len(Form) > 0) Then sAttributes = sAttributes + XMLAttribute("Form", Form, False)
      If (Len(Fields) > 0) Then sAttributes = sAttributes + XMLAttribute("Fields", Fields, False)
      If (Len(IsShare) > 0) Then sAttributes = sAttributes + XMLAttribute("IsShare", IsShare, False)
      If (Len(Subject) > 0) Then sAttributes = sAttributes + XMLAttribute("Subject", Subject, False)
      If (Len(GroupID1) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID1", GroupID1, False)
      If (Len(GroupID2) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID2", GroupID2, False)
      If (Len(GroupID3) > 0) Then sAttributes = sAttributes + XMLAttribute("GroupID3", GroupID3, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

