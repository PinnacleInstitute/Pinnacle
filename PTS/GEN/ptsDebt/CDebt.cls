Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Debt") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(DebtType) Then
         sChildren = sChildren + EnumItems(10204).XML("ptsDebtTypes", DebtType)
      Else
         sChildren = sChildren + EnumItems (10204).XML("ptsDebtTypes")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(DebtID) > 0) Then sAttributes = sAttributes + XMLAttribute("DebtID", DebtID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(DebtType) > 0) Then sAttributes = sAttributes + XMLAttribute("DebtType", DebtType, False)
      If (Len(DebtName) > 0) Then sAttributes = sAttributes + XMLAttribute("DebtName", DebtName, False)
      If (Len(Balance) > 0) Then sAttributes = sAttributes + XMLAttribute("Balance", Format$(Balance, "currency"), False)
      If (Len(Payment) > 0) Then sAttributes = sAttributes + XMLAttribute("Payment", Format$(Payment, "currency"), False)
      If (Len(MinPayment) > 0) Then sAttributes = sAttributes + XMLAttribute("MinPayment", Format$(MinPayment, "currency"), False)
      If (Len(IntRate) > 0) Then sAttributes = sAttributes + XMLAttribute("IntRate", Format$(IntRate, "#0.00"), False)
      If (Len(IntPaid) > 0) Then sAttributes = sAttributes + XMLAttribute("IntPaid", Format$(IntPaid, "currency"), False)
      If (Len(MonthsPaid) > 0) Then sAttributes = sAttributes + XMLAttribute("MonthsPaid", MonthsPaid, False)
      If (Len(IsActive) > 0) Then sAttributes = sAttributes + XMLAttribute("IsActive", IsActive, False)
      If (Len(IsConsolidate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsConsolidate", IsConsolidate, False)
      If (Len(RegInterest) > 0) Then sAttributes = sAttributes + XMLAttribute("RegInterest", RegInterest, False)
      If (Len(RegMonths) > 0) Then sAttributes = sAttributes + XMLAttribute("RegMonths", RegMonths, False)
      If (Len(AccInterest) > 0) Then sAttributes = sAttributes + XMLAttribute("AccInterest", AccInterest, False)
      If (Len(AccMonths) > 0) Then sAttributes = sAttributes + XMLAttribute("AccMonths", AccMonths, False)
      If (Len(Margin) > 0) Then sAttributes = sAttributes + XMLAttribute("Margin", Format$(Margin, "currency"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

