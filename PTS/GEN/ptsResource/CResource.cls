Public Property Get EnumItems( _ 
   ByVal bvType As ptsResourceEnumConstants) As wtSystem.CEnumItems
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns an enumerated list of items.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsResourceEnumResourceType
         oEnums.Add cptsResourceResourceTypeAll, "All"
         oEnums.Add cptsResourceResourceTypeResource1, "Resource1"
         oEnums.Add cptsResourceResourceTypeResource2, "Resource2"
         oEnums.Add cptsResourceResourceTypeResource3, "Resource3"
         oEnums.Add cptsResourceResourceTypeResource4, "Resource4"
         oEnums.Add cptsResourceResourceTypeResource5, "Resource5"
         oEnums.Add cptsResourceResourceTypeResource6, "Resource6"
         oEnums.Add cptsResourceResourceTypeResource7, "Resource7"
         oEnums.Add cptsResourceResourceTypeResource8, "Resource8"
         oEnums.Add cptsResourceResourceTypeResource9, "Resource9"
         oEnums.Add cptsResourceResourceTypeResource10, "Resource10"
         oEnums.Add cptsResourceResourceTypeResource11, "Resource11"
         oEnums.Add cptsResourceResourceTypeResource12, "Resource12"
         oEnums.Add cptsResourceResourceTypeResource13, "Resource13"
         oEnums.Add cptsResourceResourceTypeResource14, "Resource14"
         oEnums.Add cptsResourceResourceTypeResource15, "Resource15"
      Case cptsResourceEnumShare
         oEnums.Add cptsResourceShareUsed, "Used"
         oEnums.Add cptsResourceShareShared, "Shared"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

