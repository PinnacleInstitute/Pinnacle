Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Msg") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(9713).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (9713).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(MsgID) > 0) Then sAttributes = sAttributes + XMLAttribute("MsgID", MsgID, False)
      If (Len(OwnerType) > 0) Then sAttributes = sAttributes + XMLAttribute("OwnerType", OwnerType, False)
      If (Len(OwnerID) > 0) Then sAttributes = sAttributes + XMLAttribute("OwnerID", OwnerID, False)
      If (Len(AuthUserID) > 0) Then sAttributes = sAttributes + XMLAttribute("AuthUserID", AuthUserID, False)
      If (Len(NameLast) > 0) Then sAttributes = sAttributes + XMLAttribute("NameLast", NameLast, False)
      If (Len(NameFirst) > 0) Then sAttributes = sAttributes + XMLAttribute("NameFirst", NameFirst, False)
      If (Len(UserName) > 0) Then sAttributes = sAttributes + XMLAttribute("UserName", UserName, False)
      If (Len(MsgDate) > 0) And (MsgDate <> "0") Then sAttributes = sAttributes + XMLAttribute("MsgDate", Format$(MsgDate, "m-d-yy h:mmam/pm"), False)
      If (Len(Subject) > 0) Then sAttributes = sAttributes + XMLAttribute("Subject", Subject, False)
      If (Len(Message) > 0) Then sChildren = sChildren + XMLElement("Message", "", "<!-- " + Replace(Message, "--", "- ") + " -->", "")
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Num) > 0) Then sAttributes = sAttributes + XMLAttribute("Num", Num, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

Public Function XMLMsgs() As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XMLMsgs"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String

   On Error GoTo ErrorHandler

   '-----create the XML for the attributes
   sAttributes = ""
   If (Len(MsgID) > 0) Then sAttributes = sAttributes + XMLAttribute("id", MsgID, False)
   If (Len(MsgDate) > 0) And (MsgDate <> "0") Then sAttributes = sAttributes + XMLAttribute("date", Format$(MsgDate, "m-d-yy h:mmam/pm"), False)
   If (Len(Username) > 0) Then sAttributes = sAttributes + XMLAttribute("from", Username, False)
   If (Len(Subject) > 0) Then sAttributes = sAttributes + XMLAttribute("Subject", Subject, False)
   If (Len(Message) > 0) Then sChildren = sChildren + XMLElement("body", "", "<!-- " + Replace(Message, "--", "- ") + " -->", "")

   '-----create the XML for the object
   XMLMsgs = XMLElement("Msg", sAttributes, , sChildren)

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

