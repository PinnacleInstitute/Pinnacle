Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "SessionLesson") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(2415).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (2415).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(SessionLessonID) > 0) Then sAttributes = sAttributes + XMLAttribute("SessionLessonID", SessionLessonID, False)
      If (Len(SessionID) > 0) Then sAttributes = sAttributes + XMLAttribute("SessionID", SessionID, False)
      If (Len(LessonID) > 0) Then sAttributes = sAttributes + XMLAttribute("LessonID", LessonID, False)
      If (Len(LessonName) > 0) Then sAttributes = sAttributes + XMLAttribute("LessonName", LessonName, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Content) > 0) Then sAttributes = sAttributes + XMLAttribute("Content", Content, False)
      If (Len(Quiz) > 0) Then sAttributes = sAttributes + XMLAttribute("Quiz", Quiz, False)
      If (Len(MediaType) > 0) Then sAttributes = sAttributes + XMLAttribute("MediaType", MediaType, False)
      If (Len(MediaURL) > 0) Then sAttributes = sAttributes + XMLAttribute("MediaURL", MediaURL, False)
      If (Len(LessonLength) > 0) Then sAttributes = sAttributes + XMLAttribute("LessonLength", LessonLength, False)
      If (Len(PassingGrade) > 0) Then sAttributes = sAttributes + XMLAttribute("PassingGrade", PassingGrade, False)
      If (Len(QuizWeight) > 0) Then sAttributes = sAttributes + XMLAttribute("QuizWeight", QuizWeight, False)
      If (Len(IsPassQuiz) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPassQuiz", IsPassQuiz, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(QuizScore) > 0) Then sAttributes = sAttributes + XMLAttribute("QuizScore", QuizScore, False)
      If (Len(CreateDate) > 0) And (CreateDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CreateDate", Format$(CreateDate, "m/d/yyyy"), False)
      If (Len(CompleteDate) > 0) And (CompleteDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CompleteDate", Format$(CompleteDate, "m/d/yyyy"), False)
      If (Len(Time) > 0) Then sAttributes = sAttributes + XMLAttribute("Time", Time, False)
      If (Len(Times) > 0) Then sAttributes = sAttributes + XMLAttribute("Times", Times, False)
      If (Len(Questions) > 0) Then sAttributes = sAttributes + XMLAttribute("Questions", Questions, False)
      If (Len(Location) > 0) Then sAttributes = sAttributes + XMLAttribute("Location", Location, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

Public Function LessonXML() As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
