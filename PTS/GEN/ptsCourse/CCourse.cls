Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Course") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(1111).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (1111).XML("ptsStatuss")
      End If

      If IsNumeric(CourseType) Then
         sChildren = sChildren + EnumItems(1112).XML("ptsCourseTypes", CourseType)
      Else
         sChildren = sChildren + EnumItems (1112).XML("ptsCourseTypes")
      End If

      If IsNumeric(CourseLevel) Then
         sChildren = sChildren + EnumItems(1113).XML("ptsCourseLevels", CourseLevel)
      Else
         sChildren = sChildren + EnumItems (1113).XML("ptsCourseLevels")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(CourseID) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseID", CourseID, False)
      If (Len(CourseCategoryID) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseCategoryID", CourseCategoryID, False)
      If (Len(TrainerID) > 0) Then sAttributes = sAttributes + XMLAttribute("TrainerID", TrainerID, False)
      If (Len(CompanyID) > 0) Then sAttributes = sAttributes + XMLAttribute("CompanyID", CompanyID, False)
      If (Len(ExamID) > 0) Then sAttributes = sAttributes + XMLAttribute("ExamID", ExamID, False)
      If (Len(CourseCategoryName) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseCategoryName", CourseCategoryName, False)
      If (Len(TrainerName) > 0) Then sAttributes = sAttributes + XMLAttribute("TrainerName", TrainerName, False)
      If (Len(CourseName) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseName", CourseName, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(CourseType) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseType", CourseType, False)
      If (Len(CourseLevel) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseLevel", CourseLevel, False)
      If (Len(Description) > 0) Then sAttributes = sAttributes + XMLAttribute("Description", Description, False)
      If (Len(Language) > 0) Then sAttributes = sAttributes + XMLAttribute("Language", Language, False)
      If (Len(CourseLength) > 0) Then sAttributes = sAttributes + XMLAttribute("CourseLength", CourseLength, False)
      If (Len(CourseDate) > 0) And (CourseDate <> "0") Then sAttributes = sAttributes + XMLAttribute("CourseDate", Format$(CourseDate, "m/d/yyyy"), False)
      If (Len(IsPaid) > 0) Then sAttributes = sAttributes + XMLAttribute("IsPaid", IsPaid, False)
      If (Len(Price) > 0) Then sAttributes = sAttributes + XMLAttribute("Price", Format$(Price, "currency"), False)
      If (Len(Grp) > 0) Then sAttributes = sAttributes + XMLAttribute("Grp", Grp, False)
      If (Len(Seq) > 0) Then sAttributes = sAttributes + XMLAttribute("Seq", Seq, False)
      If (Len(PassingGrade) > 0) Then sAttributes = sAttributes + XMLAttribute("PassingGrade", PassingGrade, False)
      If (Len(Rating) > 0) Then sAttributes = sAttributes + XMLAttribute("Rating", Rating, False)
      If (Len(RatingCnt) > 0) Then sAttributes = sAttributes + XMLAttribute("RatingCnt", RatingCnt, False)
      If (Len(Classes) > 0) Then sAttributes = sAttributes + XMLAttribute("Classes", Classes, False)
      If (Len(Video) > 0) Then sAttributes = sAttributes + XMLAttribute("Video", Video, False)
      If (Len(Audio) > 0) Then sAttributes = sAttributes + XMLAttribute("Audio", Audio, False)
      If (Len(Quiz) > 0) Then sAttributes = sAttributes + XMLAttribute("Quiz", Quiz, False)
      If (Len(Reference) > 0) Then sAttributes = sAttributes + XMLAttribute("Reference", Reference, False)
      If (Len(ScoreFactor) > 0) Then sAttributes = sAttributes + XMLAttribute("ScoreFactor", ScoreFactor, False)
      If (Len(NoCertificate) > 0) Then sAttributes = sAttributes + XMLAttribute("NoCertificate", NoCertificate, False)
      If (Len(NoEvaluation) > 0) Then sAttributes = sAttributes + XMLAttribute("NoEvaluation", NoEvaluation, False)
      If (Len(IsCustomCertificate) > 0) Then sAttributes = sAttributes + XMLAttribute("IsCustomCertificate", IsCustomCertificate, False)
      If (Len(Credit) > 0) Then sAttributes = sAttributes + XMLAttribute("Credit", Format$(Credit, "#0.00"), False)
      If (Len(ExamWeight) > 0) Then sAttributes = sAttributes + XMLAttribute("ExamWeight", ExamWeight, False)
      If (Len(Pre) > 0) Then sAttributes = sAttributes + XMLAttribute("Pre", Pre, False)
      If (Len(Quantity) > 0) Then sAttributes = sAttributes + XMLAttribute("Quantity", Quantity, False)
      If (Len(Amount) > 0) Then sAttributes = sAttributes + XMLAttribute("Amount", Amount, False)
      If (Len(OrgID) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgID", OrgID, False)
      If (Len(OrgStatus) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgStatus", OrgStatus, False)
      If (Len(OrgSeq) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgSeq", OrgSeq, False)
      If (Len(OrgCourseID) > 0) Then sAttributes = sAttributes + XMLAttribute("OrgCourseID", OrgCourseID, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

