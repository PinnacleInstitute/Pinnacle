   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCourseEnumStatus
         oEnums.Add cptsCourseStatusPending, "Pending"
         oEnums.Add cptsCourseStatusReview, "Review"
         oEnums.Add cptsCourseStatusActive, "Active"
         oEnums.Add cptsCourseStatusInActive, "InActive"
      Case cptsCourseEnumCourseType
         oEnums.Add cptsCourseCourseTypePersonal, "Personal"
         oEnums.Add cptsCourseCourseTypeAcademic, "Academic"
         oEnums.Add cptsCourseCourseTypeProfessional, "Professional"
         oEnums.Add cptsCourseCourseTypeTechnical, "Technical"
      Case cptsCourseEnumCourseLevel
         oEnums.Add cptsCourseCourseLevelBeginner, "Beginner"
         oEnums.Add cptsCourseCourseLevelIntermediate, "Intermediate"
         oEnums.Add cptsCourseCourseLevelAdvanced, "Advanced"
         oEnums.Add cptsCourseCourseLevelExpert, "Expert"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

