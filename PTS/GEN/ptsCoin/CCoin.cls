   Const cProcName As String = "EnumItems"
   '------------------------------------------------------------------------------------------------------------------------
   Dim oEnums As wtSystem.CEnumItems

   On Error GoTo ErrorHandler

   Set oEnums = New wtSystem.CEnumItems

   Select Case bvType
      Case cptsCoinEnumStatus
         oEnums.Add cptsCoinStatusSubmitted, "Submitted"
         oEnums.Add cptsCoinStatusPending, "Pending"
         oEnums.Add cptsCoinStatusComplete, "Complete"
         oEnums.Add cptsCoinStatusRequest, "Request"
         oEnums.Add cptsCoinStatusCancelled, "Cancelled"
      Case cptsCoinEnumCoinType
         oEnums.Add cptsCoinCoinTypeMined, "Mined"
         oEnums.Add cptsCoinCoinTypeSent, "Sent"
         oEnums.Add cptsCoinCoinTypeReceived, "Received"
   End Select

   Set EnumItems = oEnums
   Set oEnums = Nothing

   Exit Property

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Property

