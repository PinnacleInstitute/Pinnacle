Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "BarterArea") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(BarterAreaType) Then
         sChildren = sChildren + EnumItems(17011).XML("ptsBarterAreaTypes", BarterAreaType)
      Else
         sChildren = sChildren + EnumItems (17011).XML("ptsBarterAreaTypes")
      End If

      If IsNumeric(Status) Then
         sChildren = sChildren + EnumItems(17012).XML("ptsStatuss", Status)
      Else
         sChildren = sChildren + EnumItems (17012).XML("ptsStatuss")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(BarterAreaID) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterAreaID", BarterAreaID, False)
      If (Len(ParentID) > 0) Then sAttributes = sAttributes + XMLAttribute("ParentID", ParentID, False)
      If (Len(CountryID) > 0) Then sAttributes = sAttributes + XMLAttribute("CountryID", CountryID, False)
      If (Len(ConsumerID) > 0) Then sAttributes = sAttributes + XMLAttribute("ConsumerID", ConsumerID, False)
      If (Len(CountryName) > 0) Then sAttributes = sAttributes + XMLAttribute("CountryName", CountryName, False)
      If (Len(BarterAreaName) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterAreaName", BarterAreaName, False)
      If (Len(BarterAreaType) > 0) Then sAttributes = sAttributes + XMLAttribute("BarterAreaType", BarterAreaType, False)
      If (Len(Status) > 0) Then sAttributes = sAttributes + XMLAttribute("Status", Status, False)
      If (Len(Children) > 0) Then sAttributes = sAttributes + XMLAttribute("Children", Children, False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

