Public Function XML( _ 
   Optional ByVal bvOption As Integer = 1000, _
   Optional ByVal bvElementName As String = "Expense") As String
   '------------------------------------------------------------------------------------------------------------------------
   '   Returns the XML for the item.
   '------------------------------------------------------------------------------------------------------------------------
   Dim ErrNo As Long, ErrSrc As String, ErrDesc As String
   Const cProcName As String = "XML"
   '------------------------------------------------------------------------------------------------------------------------
   Dim sChildren As String
   Dim sAttributes As String
   Dim sValue As String
   Dim sOption As String
   Dim bLookup As Integer
   Dim bFilter As Integer
   Dim bStatic As Integer
   Dim bAttrib As Integer

   On Error GoTo ErrorHandler

   'Handle Common Options for item classes
   Select Case bvOption
      Case 2: bvOption = 1111    ' attributes with all enums
      Case 3: bvOption = 1001    ' attributes with static enums only
   End Select
   sOption = Format(bvOption, "0000")
   bAttrib = (Mid(sOption, 1, 1) = "1")
   bFilter = (Mid(sOption, 2, 1) = "1")
   bLookup = (Mid(sOption, 3, 1) = "1")
   bStatic = (Mid(sOption, 4, 1) = "1")

   '-----create the XML for the children
   sChildren = ""
   If bStatic Then
      If IsNumeric(ExpType) Then
         sChildren = sChildren + EnumItems(6410).XML("ptsExpTypes", ExpType)
      Else
         sChildren = sChildren + EnumItems (6410).XML("ptsExpTypes")
      End If

   End If
   '-----create the XML for the attributes
   sAttributes = ""

   If bAttrib Then
      If (Len(ExpenseID) > 0) Then sAttributes = sAttributes + XMLAttribute("ExpenseID", ExpenseID, False)
      If (Len(MemberID) > 0) Then sAttributes = sAttributes + XMLAttribute("MemberID", MemberID, False)
      If (Len(ExpenseTypeID) > 0) Then sAttributes = sAttributes + XMLAttribute("ExpenseTypeID", ExpenseTypeID, False)
      If (Len(ExpenseTypeName) > 0) Then sAttributes = sAttributes + XMLAttribute("ExpenseTypeName", ExpenseTypeName, False)
      If (Len(TaxType) > 0) Then sAttributes = sAttributes + XMLAttribute("TaxType", TaxType, False)
      If (Len(IsRequired) > 0) Then sAttributes = sAttributes + XMLAttribute("IsRequired", IsRequired, False)
      If (Len(ExpType) > 0) Then sAttributes = sAttributes + XMLAttribute("ExpType", ExpType, False)
      If (Len(ExpDate) > 0) And (ExpDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ExpDate", Format$(ExpDate, "m/d/yyyy"), False)
      If (Len(Total) > 0) Then sAttributes = sAttributes + XMLAttribute("Total", Format$(Total, "currency"), False)
      If (Len(Amount) > 0) Then sAttributes = sAttributes + XMLAttribute("Amount", Format$(Amount, "currency"), False)
      If (Len(MilesStart) > 0) Then sAttributes = sAttributes + XMLAttribute("MilesStart", Format$(MilesStart, "###,##0"), False)
      If (Len(MilesEnd) > 0) Then sAttributes = sAttributes + XMLAttribute("MilesEnd", Format$(MilesEnd, "###,##0"), False)
      If (Len(TotalMiles) > 0) Then sAttributes = sAttributes + XMLAttribute("TotalMiles", Format$(TotalMiles, "###,##0"), False)
      If (Len(Note1) > 0) Then sAttributes = sAttributes + XMLAttribute("Note1", Note1, False)
      If (Len(Note2) > 0) Then sAttributes = sAttributes + XMLAttribute("Note2", Note2, False)
      If (Len(Purpose) > 0) Then sAttributes = sAttributes + XMLAttribute("Purpose", Purpose, False)
      If (Len(Method) > 0) Then sAttributes = sAttributes + XMLAttribute("Method", Method, False)
      If (Len(FromDate) > 0) And (FromDate <> "0") Then sAttributes = sAttributes + XMLAttribute("FromDate", Format$(FromDate, "m/d/yyyy"), False)
      If (Len(ToDate) > 0) And (ToDate <> "0") Then sAttributes = sAttributes + XMLAttribute("ToDate", Format$(ToDate, "m/d/yyyy"), False)
   End If
   '-----create the XML for the value
   sValue = ""

   '-----create the XML for the object
   If bAttrib Then
      XML = XMLElement("pts" + bvElementName, sAttributes, sValue, sChildren)
   Else
      XML = sChildren
   End If

   Exit Function

ErrorHandler:
   CatchError ErrNo, ErrSrc, ErrDesc, cModName, cProcName
   If Err.Number = 0 Then Resume Next
   Err.Raise ErrNo, ErrSrc, ErrDesc
End Function

